@close_or_save=\"close_or_save_dominio\" @dominio_data=\"setDomainVariables\" :domain=\"dominio_data\" />\r\n                        </v-dialog>\r\n\r\n                        <!--Formulario para criação de submódulo-->\r\n                        <v-dialog v-model=\"dialog_submodulo\" persistent=\"persistent\" max-width=\"600px\">\r\n                            <SubModuloDialog :submodule=\"submodulo\" :module=\"modulo\" @close_or_save=\"close_or_save_submodulo \" :domain=\"dominio\" />\r\n                        </v-dialog>\r\n                        <!--Formulario para criação de conceito-->\r\n                        <v-dialog v-model=\"dialog_conceito\" persistent=\"persistent\" class=\"mx-0\" max-width=\"750px\">\r\n                            <ConceitoDialog :concept=\"conceito\" :module=\"modulo\" @close_or_save=\"close_or_save_conceito\" :domain=\"dominio\" />\r\n                        </v-dialog>\r\n                    </v-col>\r\n                </v-row>\r\n            </v-expansion-panel-header>\r\n            <v-expansion-panel-content>\r\n                {{ dominio_data.subtitle }}\r\n                <!--Panels dos Módulos-->\r\n                <v-expansion-panels v-for=\"(modulo) in dominio_data.modules\" :key=\"modulo.idmodule\" class=\"mt-2 mb-2\">\r\n                    <v-expansion-panel v-if=\"modulo.fk_idmodule === null\">\r\n                        <v-expansion-panel-header color=\"primary\" style=\"color:white;\">\r\n                            <v-row>\r\n                                <v-col cols=\"9\">\r\n                                    <p>\r\n                                        [Modulo]\r\n                                        {{ modulo.namemodule }}\r\n                                    </p>\r\n                                </v-col>\r\n                                <v-col cols=\"3\">\r\n                                    <!--Formulario para adição de submódulo ou conceito-->\r\n                                    <v-menu t