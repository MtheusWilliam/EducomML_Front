AA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAoBA,gBAAA,OAAA,CAAA,GAAA;;AApBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAsBA,KAvBA;AAyBA,IAAA,QAzBA,sBAyBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,QAAA,EADA;AAAA;AAAA;AAAA;;AAEA,gBAAA,MAAA,CAAA,aAAA,GAAA,IAAA;AAFA;AAAA;AAAA,uBAIA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,QAAA,EAJA;;AAAA;AAAA;AAAA,uBAKA,MAAA,CAAA,aAAA,EALA;;AAAA;AAMA,gBAAA,MAAA,CAAA,YAAA,GAAA,gBAAA;AACA,gBAAA,MAAA,CAAA,YAAA,GAAA,0CAAA;AACA,gBAAA,MAAA,CAAA,YAAA,GACA,gHADA;AAEA,gBAAA,MAAA,CAAA,aAAA,GAAA,KAAA;AACA,gBAAA,MAAA,CAAA,WAAA,GAAA,IAAA;AAXA;AAAA;;AAAA;AAAA;AAAA;AAaA,gBAAA,MAAA,CAAA,YAAA,GAAA,cAAA;AACA,gBAAA,MAAA,CAAA,YAAA,GAAA,MAAA;AACA,gBAAA,MAAA,CAAA,YAAA,GACA,6DADA;AAEA,gBAAA,MAAA,CAAA,aAAA,GAAA,KAAA;AACA,gBAAA,MAAA,CAAA,WAAA,GAAA,IAAA;;AAlBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAqBA,KA9CA;AA+CA,IAAA,UA/CA,wBA+CA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,aAAA,EAAA;AACA,kBAAA,QAAA,EAAA,MAAA,CAAA,QADA;AAEA,kBAAA,QAAA,EAAA,MAAA,CAAA;AAFA,iBAAA,CADA;;AAAA;AAAA;AAAA,uBAKA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,kBAAA,IAAA,EAAA;AADA,iBAAA,CALA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA,KAvDA;AAwDA,IAAA,aAxDA,2BAwDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,kBAAA,IAAA,EAAA;AADA,iBAAA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA,KA5DA;AA6DA,IAAA,OA7DA,qBA6DA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAGA,GAAA,GACA,IADA,CACA,UADA,EACA;AACA,kBAAA,QAAA,EAAA,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA;AADA,iBADA,EAIA,IAJA;AAAA,qFAIA,kBAAA,QAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mCACA,GAAA,GACA,GADA,CACA,QAAA,CAAA,IAAA,CAAA,GADA,EAEA,IAFA,CAEA,UAAA,QAAA;AAAA,qCAAA,GAAA,GAAA,QAAA,CAAA,IAAA;AAAA,6BAFA,CADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mBAJA;;AAAA;AAAA;AAAA;AAAA,oBAHA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAaA,gBAAA,GAAA,GAAA,IAAA;;AAbA;AAAA,kDAeA,GAfA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgBA;AA7EA;AAlEA,CAAA","sourcesContent":["<template>\r\n  <v-card class=\"elevation-12 mx-auto\" style=\"position:relative;top:20vh;\" width=\"50vw\">\r\n    <v-toolbar color=\"black\" flat>\r\n      <v-toolbar-title style=\"color:#FFCC00;\">Crie sua conta no Educom.ML</v-toolbar-title>\r\n      <v-spacer></v-spacer>\r\n      <v-img class=\"mt-1\" src=\"../assets/logo.png\" style=\"width: 20px;\" max-width=\"200\"></v-img>\r\n      <v-tooltip bottom>\r\n        <span>Source</span>\r\n      </v-tooltip>\r\n    </v-toolbar>\r\n    <v-card-text>\r\n      <v-form style=\"margin-bottom: -20px;\" ref=\"form\">\r\n        <v-text-field\r\n          v-model=\"username\"\r\n          label=\"Username\"\r\n          name=\"username\"\r\n          :rules=\"usernameRules\"\r\n          prepend-icon=\"mdi-account\"\r\n          required\r\n        ></v-text-field>\r\n\r\n        <v-text-field\r\n          v-model=\"name\"\r\n          label=\"Nome\"\r\n          name=\"name\"\r\n          :rules=\"nameRules\"\r\n          prepend-icon=\"mdi-account\"\r\n          required\r\n        ></v-text-field>\r\n\r\n        <v-text-field\r\n          v-model=\"lastname\"\r\n          label=\"Sobrenome\"\r\n          name=\"lastname\"\r\n          :rules=\"lastnameRules\"\r\n          prepend-icon=\"mdi-account\"\r\n          required\r\n        ></v-text-field>\r\n\r\n        <v-text-field\r\n          id=\"email\"\r\n          label=\"E-mail\"\r\n          name=\"email\"\r\n          :rules=\"emailRules\"\r\n          v-model=\"email\"\r\n          prepend-icon=\"mdi-email\"\r\n          type=\"email\"\r\n          required\r\n        ></v-text-field>\r\n\r\n        <v-text-field\r\n          id=\"password\"\r\n          label=\"Senha\"\r\n          name=\"password\"\r\n          v-model=\"password\"\r\n          :rules=\"passwordRules\"\r\n          type=\"password\"\r\n          prepend-icon=\"mdi-lock\"\r\n          required\r\n        ></v-text-field>\r\n\r\n        <v-text-field\r\n          id=\"passwordConfirm\"\r\n          label=\"Confirmação de Senha\"\r\n          name=\"passwordConfirm\"\r\n          v-model=\"passwordConfirm\"\r\n          type=\"password\"\r\n          :rules=\"[passwordConfirmationRule]\"\r\n          prepend-icon=\"mdi-lock\"\r\n          required\r\n        ></v-text-field>\r\n      </v-form>\r\n    </v-card-text>\r\n    <v-card-actions>\r\n      <a @click=\"redirectLogin()\" class=\"ml-5\">\r\n        <span>Já possui uma conta? Faça seu Login</span>\r\n      </a>\r\n      <v-spacer></v-spacer>\r\n      <v-btn color=\"primary\" @click=\"validate()\" large class=\"mr-4\">Cadastrar</v-btn>\r\n    </v-card-actions>\r\n    <div class=\"text-center\">\r\n      <v-dialog v-model=\"dialogError\" width=\"550\">\r\n        <v-card>\r\n          <v-card-title :class=\"messageClass\" primary-title style=\"color:white;\">{{ messageTitle }}</v-card-title>\r\n          <v-card-text class=\"mt-3\" style=\"font-size: 1.3em;\">\r\n            {{\r\n            messageError\r\n            }}\r\n          </v-card-text>\r\n          <v-divider></v-divider>\r\n          <v-card-actions>\r\n            <v-spacer></v-spacer>\r\n            <v-btn color=\"primary\" text @click=\"dialogError = false\">Ok</v-btn>\r\n          </v-card-actions>\r\n        </v-card>\r\n      </v-dialog>\r\n    </div>\r\n    <div class=\"text-center\">\r\n      <v-dialog v-model=\"dialogLoading\" max-width=\"290\" persistent=\"persistent\">\r\n        <v-card color=\"primary\" dark>\r\n          <v-card-text style=\"color:white;\">\r\n            <v-row class=\"pt-2 pb-3\">\r\n              <br />\r\n              <v-spacer></v-spacer>\r\n              <span style=\"font-size: 1.3em; color:white;\">{{ dialogLoadingMessage }}</span>\r\n              <v-spacer></v-spacer>\r\n            </v-row>\r\n            <v-row>\r\n              <v-spacer></v-spacer>\r\n              <v-progress-circular indeterminate color=\"white\" class=\"mb-0\"></v-progress-circular>\r\n              <v-spacer></v-spacer>\r\n            </v-row>\r\n          </v-card-text>\r\n        </v-card>\r\n      </v-dialog>\r\n    </div>\r\n  </v-card>\r\n</template>\r\n\r\n<script>\r\nimport Api from \"@/services/Api\";\r\nexport default {\r\n  name: \"SignUp\",\r\n  data() {\r\n    return {\r\n      dialog: false,\r\n      dialogLoading: false,\r\n      dialogLoadingMessage: \"Enviando e-mail de confirmação\",\r\n      username: \"\",\r\n      usernameRules: [\r\n        (v) => !!v || \"É necessário inserir seu username\",\r\n        (v) =>\r\n          (v && v.length <= 25) ||\r\n          \"Seu username deve possuir menos de 25 caracteres\",\r\n      ],\r\n      name: \"\",\r\n      nameRules: [\r\n        (v) => !!v || \"É necessário inserir seu nome\",\r\n        (v) =>\r\n          (v && v.length <= 30) ||\r\n          \"Seu nome deve possuir menos de 30 caracteres\",\r\n      ],\r\n      lastname: \"\",\r\n      lastnameRules: [\r\n        (v) => !!v || \"É necessário inserir seu sobrenome\",\r\n        (v) =>\r\n          (v && v.length <= 30) ||\r\n          \"Seu sobrenome deve possuir menos de 30 caracteres\",\r\n      ],\r\n      email: \"\",\r\n      emailRules: [\r\n        (v) => !!v || \"É necessário inserir seu e-mail\",\r\n        (v) => /.+@.+\\..+/.test(v) || \"É necessário inserir um e-mail válido\",\r\n      ],\r\n      password: \"\",\r\n      passwordRules: [\r\n        (v) => !!v || \"É necessário inserir sua senha\",\r\n        (v) =>\r\n          (v && v.length <= 8) ||\r\n          \"Sua senha deve possuir no mínimo 8 caracteres\",\r\n      ],\r\n      passwordConfirm: \"\",\r\n      user_url: \"\",\r\n      messageClass: \"\",\r\n      messageTitle: \"\",\r\n      messageError: \"\",\r\n      dialogError: \"\",\r\n    };\r\n  },\r\n  computed: {\r\n    passwordConfirmationRule() {\r\n      return () =>\r\n        this.password === this.passwordConfirm || \"As senhas devem ser iguais\";\r\n    },\r\n  },\r\n  async mounted() {\r\n    var usr = await this.getUser();\r\n    console.log(usr.password);\r\n    if (usr) {\r\n      this.user_url = usr.url;\r\n      this.username = usr.username.toLowerCase();\r\n      this.email = usr.email;\r\n      this.first_name = usr.name;\r\n      this.last_name = usr.lastname;\r\n      this.password = usr.password;\r\n    }\r\n  },\r\n  methods: {\r\n    async postOrPutUser() {\r\n      var usr = {\r\n        username: this.username.toLowerCase(),\r\n        email: this.email,\r\n        first_name: this.name,\r\n        last_name: this.lastname,\r\n        password: this.password,\r\n        is_active: false,\r\n      };\r\n      var auth = {\r\n        auth: {\r\n          username: \"admin\",\r\n          password: \"admin\",\r\n        },\r\n      };\r\n\r\n      try {\r\n        if (this.user_url) await Api().put(this.user_url, usr, auth);\r\n        else await Api().post(\"/users/\", usr, auth);\r\n      } catch (err) {\r\n        console.log(err);\r\n      }\r\n    },\r\n\r\n    async validate() {\r\n      if (this.$refs.form.validate()) {\r\n        this.dialogLoading = true;\r\n        try {\r\n          await this.$refs.form.validate();\r\n          await this.postOrPutUser();\r\n          this.messageClass = \"headline green\";\r\n          this.messageTitle = \"Email de confirmação enviado com sucesso\";\r\n          this.messageError =\r\n            \"Enviamos um email de confirmação para você. Verifique a caixa de mensagens do seu email para ativar sua conta.\";\r\n          this.dialogLoading = false;\r\n          this.dialogError = true;\r\n        } catch (err) {\r\n          this.messageClass = \"headline red\";\r\n          this.messageTitle = \"Erro\";\r\n          this.messageError =\r\n            \"O username ou email já estão sendo utilizados na plataforma\";\r\n          this.dialogLoading = false;\r\n          this.dialogError = true;\r\n        }\r\n      }\r\n    },\r\n    async firstLogin() {\r\n      await this.$store.dispatch(\"obtainToken\", {\r\n        username: this.username,\r\n        password: this.password,\r\n      });\r\n      await this.$router.push({\r\n        name: \"home\",\r\n      });\r\n    },\r\n    async redirectLogin() {\r\n      this.$router.push({\r\n        name: \"login\",\r\n      });\r\n    },\r\n    async getUser() {\r\n      var usr;\r\n      try {\r\n        await Api()\r\n          .post(\"/userId/\", {\r\n            username: this.$store.state.username,\r\n          })\r\n          .then(async (response) => {\r\n            await Api()\r\n              .get(response.data.url)\r\n              .then((response) => (usr = response.data));\r\n          });\r\n      } catch (error) {\r\n        usr = null;\r\n      }\r\n      return usr;\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components"}]}