AAAA;;AACA,aAAA;AAAA,eACA,KAAA,CAAA,QAAA,KAAA,KAAA,CAAA,eAAA,IAAA,4BADA;AAAA,OAAA;AAEA;AAJA,GAVA;AAgBA,EAAA,OAAA,EAAA;AACA,IAAA,QADA,sBACA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CACA,IADA,CAEA,sDAFA,EAGA;AACA,kBAAA,QAAA,EAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,QADA;AAEA,kBAAA,QAAA,EAAA,MAAA,CAAA;AAFA,iBAHA,EAOA;AACA,kBAAA,OAAA,EAAA;AACA,oBAAA,aAAA,EAAA,SAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA;AADA;AADA,iBAPA,EAaA,IAbA,CAaA,UAAA,QAAA,EAAA;AACA,kBAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,aAAA,EAAA,QAAA,CAAA,IAAA,CAAA,KAAA;;AACA,kBAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,gBAAA,EAAA,QAAA,CAAA,IAAA,CAAA,QAAA;;AACA,kBAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,OAAA;AACA,iBAjBA,CADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmBA;AApBA;AAhBA,CAAA","sourcesContent":["<template>\n  <v-card class=\"elevation-12\">\n    <v-toolbar color=\"black\" flat>\n      <v-toolbar-title style=\"color:#FFCC00;\">Crie sua conta no Educom.ML</v-toolbar-title>\n      <v-spacer></v-spacer>\n      <v-img class=\"mt-1\" src=\"../assets/logo.png\" style=\"width: 20px;\"></v-img>\n      <v-tooltip bottom>\n        <span>Source</span>\n      </v-tooltip>\n    </v-toolbar>\n    <v-card-text>\n      <v-form style=\"margin-bottom: -20px;\" ref=\"form\">\n        <v-text-field\n          id=\"password\"\n          label=\"Senha\"\n          name=\"password\"\n          v-model=\"password\"\n          :rules=\"passwordRules\"\n          type=\"password\"\n          prepend-icon=\"mdi-lock\"\n          required\n        ></v-text-field>\n\n        <v-text-field\n          id=\"passwordConfirm\"\n          label=\"Confirmação de Senha\"\n          name=\"passwordConfirm\"\n          v-model=\"passwordConfirm\"\n          type=\"password\"\n          :rules=\"[passwordConfirmationRule]\"\n          prepend-icon=\"mdi-lock\"\n          required\n        ></v-text-field>\n      </v-form>\n    </v-card-text>\n    <v-card-actions>\n      <v-spacer></v-spacer>\n      {{ messageError }}\n      <v-btn color=\"primary\" @click=\"validate()\" large class=\"mr-4\">Resetar Senha</v-btn>\n    </v-card-actions>\n  </v-card>\n</template>\n\n<script>\nexport default {\n  name: \"ResetPassword\",\n  data() {\n    return {\n      password: \"\",\n      passwordRules: [v => !!v || \"É necessário inserir sua senha\"],\n      passwordConfirm: \"\",\n      messageError: \"\"\n    };\n  },\n  computed: {\n    passwordConfirmationRule() {\n      return () =>\n        this.password === this.passwordConfirm || \"As senhas devem ser iguais\";\n    }\n  },\n  methods: {\n    async validate() {\n      await this.axios\n        .post(\n          \"https://educomml-back.herokuapp.com/update-password/\",\n          {\n            username: this.$route.params.username,\n            password: this.password\n          },\n          {\n            headers: {\n              Authorization: \"JWT \" + this.$route.params.token\n            }\n          }\n        )\n        .then(response => {\n          this.$store.commit(\"updateToken\", response.data.token);\n          this.$store.commit(\"updateUsername\", response.data.username);\n          this.$router.push(\"/home\");\n        });\n    }\n  }\n};\n</script>\n"],"sourceRoot":"src/components"}]}