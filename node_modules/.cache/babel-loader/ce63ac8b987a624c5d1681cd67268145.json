ACA,mDAAA;AADA;AADA,iBANA,EAYA,IAZA,CAYA,UAAA,QAAA,EAAA;AACA,sBAAA,QAAA,CAAA,IAAA,CAAA,MAAA,EAAA;AACA,oBAAA,KAAA,CAAA,YAAA,GAAA,gBAAA;AACA,oBAAA,KAAA,CAAA,YAAA,GAAA,gBAAA;AACA,mBAHA,MAGA;AACA,oBAAA,KAAA,CAAA,YAAA,GAAA,+BAAA;AACA,oBAAA,KAAA,CAAA,YAAA,GAAA,cAAA;AACA;;AACA,kBAAA,KAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA,kBAAA,KAAA,CAAA,WAAA,GAAA,IAAA;AACA,iBAtBA,CAFA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA,uBA2BA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,aAAA,EAAA;AACA,kBAAA,QAAA,EAAA,KAAA,CAAA,QADA;AAEA,kBAAA,QAAA,EAAA,KAAA,CAAA;AAFA,iBAAA,CA3BA;;AAAA;AAAA;AAAA,uBA+BA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,kBAAA,IAAA,EAAA;AADA,iBAAA,CA/BA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAmCA,gBAAA,KAAA,CAAA,YAAA,GAAA,cAAA;AACA,gBAAA,KAAA,CAAA,YAAA,GAAA,gBAAA;AACA,gBAAA,KAAA,CAAA,OAAA,GACA,kFADA;AAEA,gBAAA,KAAA,CAAA,WAAA,GAAA,IAAA;;AAvCA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA0CA;AA3CA;AAlBA,CAAA","sourcesContent":["<template>\r\n  <v-card class=\"elevation-12\">\r\n    <v-toolbar color=\"black\" flat>\r\n      <v-toolbar-title style=\"color:#FFCC00;\">Faça seu login no Educom.ML</v-toolbar-title>\r\n      <v-spacer></v-spacer>\r\n      <v-img class=\"mt-1\" src=\"../assets/logo.png\" style=\"width: 20px;\"></v-img>\r\n      <v-tooltip bottom>\r\n        <span>Source</span>\r\n      </v-tooltip>\r\n    </v-toolbar>\r\n    <v-card-text>\r\n      <v-form style=\"margin-bottom: -20px;\">\r\n        <v-text-field\r\n          v-if=\"!resetPassword\"\r\n          v-model=\"username\"\r\n          label=\"Username\"\r\n          name=\"login\"\r\n          prepend-icon=\"mdi-account\"\r\n          type=\"text\"\r\n          required\r\n        ></v-text-field>\r\n\r\n        <v-text-field\r\n          v-if=\"!resetPassword\"\r\n          id=\"password\"\r\n          label=\"Senha\"\r\n          name=\"password\"\r\n          v-model=\"password\"\r\n          prepend-icon=\"mdi-lock\"\r\n          type=\"password\"\r\n          required\r\n        ></v-text-field>\r\n\r\n        <v-text-field\r\n          v-if=\"resetPassword\"\r\n          id=\"email\"\r\n          label=\"E-mail\"\r\n          name=\"email\"\r\n          :rules=\"emailRules\"\r\n          v-model=\"email\"\r\n          prepend-icon=\"mdi-email\"\r\n          type=\"email\"\r\n          required\r\n        ></v-text-field>\r\n      </v-form>\r\n    </v-card-text>\r\n    <v-card-actions>\r\n      <a @click=\"resetPassword = !resetPassword;\" class=\"ml-5\">\r\n        <span v-if=\"!resetPassword\">Esqueceu a senha?</span>\r\n        <span v-else>Voltar</span>\r\n      </a>\r\n      <v-spacer></v-spacer>\r\n      <v-btn color=\"primary\" @click=\"login\" large class=\"mr-4\">\r\n        <span v-if=\"!resetPassword\">Login</span>\r\n        <span v-else>Enviar Email</span>\r\n      </v-btn>\r\n    </v-card-actions>\r\n    <div class=\"text-center\">\r\n      <v-dialog v-model=\"dialogError\" width=\"500\">\r\n        <v-card>\r\n          <v-card-title :class=\"messageClass\" primary-title style=\"color:white;\">{{messageTitle}}</v-card-title>\r\n          <v-card-text class=\"mt-3\" style=\"font-size: 1.3em;\">{{message}}</v-card-text>\r\n          <v-divider></v-divider>\r\n          <v-card-actions>\r\n            <v-spacer></v-spacer>\r\n            <v-btn color=\"primary\" text @click=\"dialogError = false\">Ok</v-btn>\r\n          </v-card-actions>\r\n        </v-card>\r\n      </v-dialog>\r\n    </div>\r\n  </v-card>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Login\",\r\n  data() {\r\n    return {\r\n      username: \"\",\r\n      password: \"\",\r\n      email: \"\",\r\n      emailRules: [\r\n        v => !!v || \"É necessário inserir seu e-mail\",\r\n        v => /.+@.+\\..+/.test(v) || \"É necessário inserir um e-mail válido\"\r\n      ],\r\n      resetPassword: false,\r\n      dialogError: false,\r\n      messageClass: \"\",\r\n      messageTitle: \"\",\r\n      message: \"\"\r\n    };\r\n  },\r\n  methods: {\r\n    async login() {\r\n      if (this.resetPassword) {\r\n        await this.axios\r\n          .post(\r\n            \"http://localhost:8000/reset-password/\",\r\n            {\r\n              email: this.email\r\n            },\r\n            {\r\n              headers: {\r\n                \"Access-Control-Allow-Origin\": \"*\"\r\n              }\r\n            }\r\n          )\r\n          .then(response => {\r\n            if (response.data.status) {\r\n              this.messageTitle = \"Email enviado!\";\r\n              this.messageClass = \"headline green\";\r\n            } else {\r\n              this.messageTitle = \"Erro na redefinição de senha!\";\r\n              this.messageClass = \"headline red\";\r\n            }\r\n            this.message = response.data.message;\r\n            this.dialogError = true;\r\n          });\r\n      } else {\r\n        try {\r\n          await this.$store.dispatch(\"obtainToken\", {\r\n            username: this.username,\r\n            password: this.password\r\n          });\r\n          await this.$router.push({\r\n            name: \"home\"\r\n          });\r\n        } catch (err) {\r\n          this.messageClass = \"headline red\";\r\n          this.messageTitle = \"Erro no Login!\";\r\n          this.message =\r\n            \"Verifique login e senha inseridos e se o cadastro da sua conta já foi confirmada\";\r\n          this.dialogError = true;\r\n        }\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>"],"sourceRoot":"src/components"}]}