AFA,EAEA,IAFA,CAAA;AAGA,SANA,MAMA;AACA,eAAA,WAAA;AACA;;AACA,aAAA,KAAA,CAAA,eAAA,EAAA,MAAA;AACA;;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,KAAA;AACA,KAtFA;AAuFA,IAAA,KAvFA,mBAuFA;AACA,WAAA,KAAA,CAAA,eAAA,EAAA,OAAA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,KAAA;AACA,KA1FA;AA2FA,IAAA,eA3FA,6BA2FA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,eAAA;AACA,KA7FA;;AA8FA;;;;;;;;;;;;;;;;;AAkBA,IAAA,KAhHA,mBAgHA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,EAAA;AACA,KAlHA;AAmHA,IAAA,UAnHA,wBAmHA;AACA,WAAA,aAAA,CAAA,IAAA,CAAA,EAAA;AACA,WAAA,WAAA,CAAA,IAAA,CAAA,EAAA;AACA,WAAA,WAAA,CAAA,IAAA,CAAA,EAAA;AACA,WAAA,kBAAA;AACA,KAxHA;AAyHA,IAAA,aAzHA,yBAyHA,SAzHA,EAyHA;AACA,UAAA,KAAA,WAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,aAAA,kBAAA;;AACA,YAAA,SAAA,IAAA,CAAA,EAAA;AACA,eAAA,aAAA,CAAA,KAAA;AACA,eAAA,WAAA,CAAA,KAAA;AACA,eAAA,WAAA,CAAA,KAAA;AACA,SAJA,MAIA;AACA,eAAA,aAAA,CAAA,MAAA,CAAA,SAAA,EAAA,CAAA;AACA,eAAA,WAAA,CAAA,MAAA,CAAA,SAAA,EAAA,CAAA;AACA,eAAA,WAAA,CAAA,MAAA,CAAA,SAAA,EAAA,CAAA;AACA;AACA;AACA;AAtIA;AAxCA,CAAA","sourcesContent":["<template>\r\n  <v-container class=\"pa-0; ml-0\">\r\n    <v-row>\r\n      <v-card min-height=\"530px\">\r\n        <v-card-title style=\"background-color:#63B0B0; color:white;\">\r\n          <span class=\"headline\">\r\n            <p>Defina o conceito do módulo, bem como seu(s) relacionamento(s).</p>\r\n          </span>\r\n        </v-card-title>\r\n        <v-card-text>\r\n          <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\r\n            <v-text-field\r\n              v-model=\"conceptName\"\r\n              :counter=\"25\"\r\n              :rules=\"conceptNameRules\"\r\n              label=\"Nome do conceito\"\r\n              required\r\n              class=\"mt-2 mb-4\"\r\n            ></v-text-field>\r\n            <div style=\"width:102.5%; position:relative;right:1.2%;margin-top:-1.3%;\">\r\n              <v-app-bar color=\"#D2A64D\" dense dark height=\"45px\">\r\n                <v-toolbar-title style=\"font-size:1.2em;\">\r\n                  <strong>Lista de relacionamentos</strong>\r\n                </v-toolbar-title>\r\n              </v-app-bar>\r\n            </div>\r\n            <v-row class=\"mt-2\" style=\"height:50px;\">\r\n              <v-col>\r\n                <strong>\r\n                  <p>Conceito</p>\r\n                </strong>\r\n              </v-col>\r\n              <v-col>\r\n                <strong>\r\n                  <p>Nome da Relação</p>\r\n                </strong>\r\n              </v-col>\r\n              <v-col>\r\n                <strong>\r\n                  <p>Tipo da Relação</p>\r\n                </strong>\r\n              </v-col>\r\n              <v-col cols=\"1\">\r\n                <v-btn icon=\"icon\" class=\"mb-1\">\r\n                  <v-icon large class=\"mb-1\" color=\"primary\" @click=\"addRelacao()\">mdi-plus-box</v-icon>\r\n                </v-btn>\r\n              </v-col>\r\n            </v-row>\r\n            <hr />\r\n            <v-row>\r\n              <v-container>\r\n                <v-row v-for=\"(item,i) in relationForControl\" :key=\"i\">\r\n                  <v-col>\r\n                    <v-select\r\n                      v-model=\"conceptSelect[i]\"\r\n                      @change=\"teste\"\r\n                      :items=\"items\"\r\n                      label=\"Conceito\"\r\n                      style=\"margin:0px;\"\r\n                    ></v-select>\r\n                  </v-col>\r\n                  <v-col>\r\n                    <v-text-field\r\n                      v-model=\"relacaoName[i]\"\r\n                      counter=\"15\"\r\n                      label=\"Nome da Relação\"\r\n                      style=\"margin:0px;\"\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                  <v-col>\r\n                    <v-select\r\n                      v-model=\"relacaoType[i]\"\r\n                      :items=\"relacaoTypes\"\r\n                      label=\"Tipo da Relação\"\r\n                      style=\"margin:0px;\"\r\n                    ></v-select>\r\n                  </v-col>\r\n                  <v-col cols=\"1\" class=\"mt-2\">\r\n                    <v-btn icon=\"icon\" class=\"mb-1\">\r\n                      <v-icon large class=\"mb-1\" color=\"red\" @click=\"deletaRelacao(i)\">mdi-minus-box</v-icon>\r\n                    </v-btn>\r\n                  </v-col>\r\n                </v-row>\r\n              </v-container>\r\n            </v-row>\r\n          </v-form>\r\n        </v-card-text>\r\n        <v-card-actions>\r\n          <v-spacer></v-spacer>\r\n          <v-btn color=\"red\" height=\"49\" dark large @click=\"reset\">\r\n            Close\r\n            <v-icon dark right>mdi-close</v-icon>\r\n          </v-btn>\r\n          <v-btn color=\"success\" height=\"49\" dark large @click=\"validate\">\r\n            Save\r\n            <v-icon dark right>mdi-content-save</v-icon>\r\n          </v-btn>\r\n        </v-card-actions>\r\n      </v-card>\r\n    </v-row>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"ConceitoDialog\",\r\n  props: [\"domain\", \"module\", \"concept\"],\r\n  data: () => ({\r\n    auxConceito: \"\",\r\n    relationForControl: 1,\r\n    valid: true,\r\n    items: [],\r\n    conceptSelect: [\"\"],\r\n    auxConceptSelect: [\"\"],\r\n    conceptName: \"\",\r\n    conceptNameRules: [\r\n      v => !!v || \"É necessário descrever o nome do conceito\",\r\n      v =>\r\n        (v && v.length <= 25) ||\r\n        \"Nome do conceito deve ter no máximo 25 caracteres\"\r\n    ],\r\n    relacaoName: [\"\"],\r\n    relacaoNameRules: [\r\n      v => !!v || \"Necessário descrever o nome da relação\",\r\n      v =>\r\n        (v && v.length <= 15) ||\r\n        \"O nome da relação deve ter no máximo 15 caracteres\"\r\n    ],\r\n    relacaoType: [\"\"],\r\n    relacaoTypes: [\"typeOf\", \"partOf\"]\r\n  }),\r\n  watch: {\r\n    module: function() {\r\n      this.items = [];\r\n      if (this.module.concepts) {\r\n        this.module.concepts.forEach(element => {\r\n          this.items.push({\r\n            text: element.nameconcept,\r\n            value: element\r\n          });\r\n        });\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    postConceito() {\r\n      var vm = this;\r\n      axios\r\n        .post(\r\n          `http://localhost:8000/concept/`,\r\n          {\r\n            nameconcept: this.conceptName,\r\n            fk_idknowledgedomain: this.domain.url,\r\n            fk_idmodule: this.module.url\r\n          },\r\n          {\r\n            auth: {\r\n              username: \"admin\",\r\n              password: \"admin\"\r\n            }\r\n          }\r\n        )\r\n        .then(function(resposta) {\r\n          vm.auxConceito = resposta.data.url;\r\n        });\r\n    },\r\n    postRelacoes() {\r\n      var i;\r\n      var vm = this;\r\n      //console.log(\"aux referencia\", vm.auxReferencia);\r\n      for (i = 0; i < vm.conceptSelect.length; i++) {\r\n        var auxReferencia = 0;\r\n        if (this.relacaoType[i] == \"typeOf\") {\r\n          auxReferencia = 1;\r\n        } else if (this.relacaoType[i] == \"partOf\") {\r\n          auxReferencia = 2;\r\n        }\r\n        console.log(\"AUXCONCEPT URL\", auxReferencia);\r\n        /*\r\n        axios.post(\r\n          `http://localhost:8000/reference/`,\r\n          {\r\n            namereference: this.relacaoName[i],\r\n            sourceconcept: this.auxConceito,\r\n            targetconcept: this.conceptSelect[i].url,\r\n            fk_referencetype:\r\n              `http://localhost:8000/referencetype/` + auxReferencia + \"/\"\r\n          },\r\n          {\r\n            auth: {\r\n              username: \"admin\",\r\n              password: \"admin\"\r\n            }\r\n          }\r\n        )*/\r\n      }\r\n    },\r\n    putConceito() {\r\n      axios.put(\r\n        \"http://127.0.0.1:8000/concept/\" + this.concept.idconcept + \"/\",\r\n        {\r\n          nameconcept: this.conceptName,\r\n          fk_idknowledgedomain: this.domain.url,\r\n          fk_idmodule: this.module.url\r\n        },\r\n        {\r\n          auth: {\r\n            username: \"admin\",\r\n            password: \"admin\"\r\n          }\r\n        }\r\n      );\r\n    },\r\n    validate() {\r\n      var vm = this;\r\n      console.log(\"VALIDADE\", vm);\r\n      if (this.$refs.form.validate()) {\r\n        this.$refs.form.validate();\r\n        if (this.concept === \"\") {\r\n          vm.auxConceptSelect = vm.conceptSelect;\r\n          this.postConceito();\r\n          setTimeout(function() {\r\n            vm.postRelacoes();\r\n          }, 1000);\r\n        } else {\r\n          this.putConceito();\r\n        }\r\n        this.$emit(\"close_or_save\", \"save\");\r\n      }\r\n      this.$refs.form.reset();\r\n    },\r\n    reset() {\r\n      this.$emit(\"close_or_save\", \"close\");\r\n      this.$refs.form.reset();\r\n    },\r\n    resetValidation() {\r\n      this.$refs.form.resetValidation();\r\n    },\r\n    /*\r\n    validateRelacao() {\r\n      //VALIDAÇÃO DA CRIAÇÃO DAS RELAÇÕES -REFERENCIAS REPETIDAS -CAMPOS VAZIOS\r\n\r\n      var i;\r\n      for (i = 0; i < this.conceptName.length; i++) {\r\n        if (this.conceptSelect[i] && this.relacaoType[i]) {\r\n          console.log(\"Referencia Nº\", i, \" Tudo OK!\");\r\n        } else {\r\n          console.log(\r\n            \"Referencia Nº\",\r\n            i,\r\n            \" deu ERRO! Possui campo(s) vazio(s)\"\r\n          );\r\n        }\r\n      }\r\n    },\r\n    */\r\n    teste() {\r\n      console.log(\"\");\r\n    },\r\n    addRelacao() {\r\n      this.conceptSelect.push(\"\");\r\n      this.relacaoName.push(\"\");\r\n      this.relacaoType.push(\"\");\r\n      this.relationForControl++;\r\n    },\r\n    deletaRelacao(idRelacao) {\r\n      if (this.relacaoName.length > 1) {\r\n        this.relationForControl--;\r\n        if (idRelacao == 0) {\r\n          this.conceptSelect.shift();\r\n          this.relacaoName.shift();\r\n          this.relacaoType.shift();\r\n        } else {\r\n          this.conceptSelect.splice(idRelacao, 1);\r\n          this.relacaoName.splice(idRelacao, 1);\r\n          this.relacaoType.splice(idRelacao, 1);\r\n        }\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components/concept_model"}]}