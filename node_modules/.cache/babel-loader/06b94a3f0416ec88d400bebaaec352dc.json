AFA,CAEA;AAAA;AAAA;AACA;;AAEA,6BALA,CAJA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mBAFA;;AAAA;AAAA;AAAA;AAAA,oBAzFA;;AAAA;AAAA;AAAA;;AAAA;AAAA,sBAsGA,KAAA,CAAA,IAAA,KAAA,SAAA,IAAA,KAAA,CAAA,IAAA,KAAA,QAtGA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAwGA,GAAA,GACA,IADA,kBACA,WADA,EAEA,IAFA,CAEA;AAAA;AAAA;AACA;;AAEA,iBALA,CAxGA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgHA,KArJA;AAsJA,IAAA,QAtJA,sBAsJA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,EADA,GACA,MADA;;AAAA,sBAEA,MAAA,CAAA,cAAA,KAAA,IAAA,IAAA,MAAA,CAAA,eAAA,KAAA,IAFA;AAAA;AAAA;AAAA;;AAGA,gBAAA,MAAA,CAAA,UAAA,GAAA,MAAA,CAAA,eAAA,CAAA,SAAA,CAAA,UAAA,KAAA,EAAA;AACA,yBAAA,KAAA,KAAA,EAAA,CAAA,UAAA;AACA,iBAFA,CAAA;AAGA,gBAAA,MAAA,CAAA,SAAA,GAAA,MAAA,CAAA,cAAA,CAAA,SAAA,CAAA,UAAA,KAAA,EAAA;AACA,yBAAA,KAAA,KAAA,EAAA,CAAA,SAAA;AACA,iBAFA,CAAA;AAGA,gBAAA,MAAA,CAAA,YAAA,GAAA,MAAA,CAAA,sBAAA,CAAA,SAAA,CAAA,UACA,KADA,EAEA;AACA,yBAAA,KAAA,KAAA,EAAA,CAAA,YAAA;AACA,iBAJA,CAAA;AATA;AAAA,uBAcA,MAAA,CAAA,oBAAA,EAdA;;AAAA;AAAA;AAAA,uBAeA,MAAA,CAAA,KAAA,CAAA,OAAA,CAfA;;AAAA;AAAA;AAAA,uBAiBA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,KAAA,EAjBA;;AAAA;AAAA;AAAA,uBAkBA,MAAA,CAAA,cAAA,EAlBA;;AAAA;AAAA;AAAA;;AAAA;AAoBA,gBAAA,MAAA,CAAA,YAAA,GAAA,IAAA;;AApBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAsBA,KA5KA;AA6KA,IAAA,cA7KA,4BA6KA;AACA,WAAA,cAAA,GAAA,IAAA;AACA,WAAA,eAAA,GAAA,KAAA;AACA,WAAA,UAAA,GAAA,EAAA;AACA,WAAA,SAAA,GAAA,EAAA;AACA,WAAA,YAAA,GAAA,EAAA;AACA,WAAA,UAAA,GAAA,EAAA;AACA,WAAA,YAAA,GAAA,EAAA;AACA,WAAA,aAAA,GAAA,EAAA;AACA,KAtLA;AAuLA,IAAA,KAvLA,mBAuLA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,OAAA,CADA;;AAAA;AAAA;AAAA,uBAEA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,KAAA,EAFA;;AAAA;AAAA;AAAA,uBAGA,MAAA,CAAA,cAAA,EAHA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA;AA3LA;AA3BA,CAAA","sourcesContent":["<template>\r\n  <v-container class=\"pa-0\">\r\n    <v-card>\r\n      <v-card-title style=\"background-color:#63B0B0; color:white;\">\r\n        <span class=\"headline\">\r\n          <p>Escreva seu texto aqui.</p>\r\n        </span>\r\n      </v-card-title>\r\n      <v-card-text>\r\n        <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\r\n          <v-row>\r\n            <v-col cols=\"4\" v-if=\"type === 'conceito'\">\r\n              <label for=\"infoItemClassesSelect\"\r\n                >Classifique o item de informação</label\r\n              >\r\n              <v-select\r\n                id=\"infoItemClassesSelect\"\r\n                v-model=\"infoClasse\"\r\n                :items=\"infoItemClasses\"\r\n                label=\"Classe\"\r\n                style=\"margin:0px;\"\r\n              ></v-select>\r\n            </v-col>\r\n            <v-col cols=\"4\">\r\n              <label for=\"infoItemLevelsSelect\"\r\n                >Qual o nível de dificuldade deste conteúdo?</label\r\n              >\r\n              <v-select\r\n                id=\"infoItemLevelsSelect\"\r\n                v-model=\"infoLevel\"\r\n                :items=\"infoItemLevels\"\r\n                label=\"Nível\"\r\n                style=\"margin:0px;\"\r\n              ></v-select>\r\n            </v-col>\r\n            <v-col cols=\"4\">\r\n              <label for=\"infoItemLearningStylesSelect\"\r\n                >Qual o estilo de aprendizado deste conteúdo?</label\r\n              >\r\n              <v-select\r\n                id=\"infoItemLearningStylesSelect\"\r\n                v-model=\"infoLearning\"\r\n                :items=\"infoItemLearningStyles\"\r\n                label=\"Nível\"\r\n                style=\"margin:0px;\"\r\n              ></v-select>\r\n            </v-col>\r\n          </v-row>\r\n          <br />\r\n          <v-row style=\"margin-bottom: -35px;\">\r\n            <v-col cols=\"11\">\r\n              <label for=\"textFullArea\">Seu texto na forma não-resumida:</label>\r\n              <v-textarea\r\n                id=\"textFullArea\"\r\n                background-color=\"#F2F3F3\"\r\n                clearable\r\n                clear-icon=\"mdi-close-circle\"\r\n                rows=\"12\"\r\n                filled\r\n                auto-grow\r\n                v-model=\"infoTextFull\"\r\n              ></v-textarea>\r\n            </v-col>\r\n            <v-col cols=\"1\" style=\"red; padding-top: 20%;\">\r\n              <v-switch v-model=\"switchTextFull\"></v-switch>\r\n            </v-col>\r\n          </v-row>\r\n          <v-row style=\"margin-bottom: -30px;\">\r\n            <v-col cols=\"11\">\r\n              <label for=\"textShortArea\">Seu texto na forma resumida:</label>\r\n              <v-textarea\r\n                id=\"textShortArea\"\r\n                background-color=\"#F2F3F3\"\r\n                clearable\r\n                clear-icon=\"mdi-close-circle\"\r\n                class=\"mt-2\"\r\n                rows=\"5\"\r\n                filled\r\n                auto-grow\r\n                v-model=\"infoTextShort\"\r\n              ></v-textarea>\r\n            </v-col>\r\n            <v-col cols=\"1\" style=\"padding-top: 70px;\">\r\n              <v-switch v-model=\"switchTextShort\"></v-switch>\r\n            </v-col>\r\n          </v-row>\r\n        </v-form>\r\n      </v-card-text>\r\n      <v-card-actions>\r\n        <v-spacer></v-spacer>\r\n        <v-btn color=\"red\" height=\"49\" dark large @click=\"reset\">\r\n          Cancelar\r\n          <v-icon dark right>mdi-close</v-icon>\r\n        </v-btn>\r\n        <v-btn color=\"success\" height=\"49\" dark large @click=\"validate\">\r\n          Salvar\r\n          <v-icon dark right>mdi-content-save</v-icon>\r\n        </v-btn>\r\n      </v-card-actions>\r\n    </v-card>\r\n    <div class=\"text-center\">\r\n      <v-dialog v-model=\"dialog_alert\" width=\"500\">\r\n        <v-card>\r\n          <v-card-title class=\"headline red\" primary-title style=\"color:white;\"\r\n            >ALERTA!</v-card-title\r\n          >\r\n          <v-card-text class=\"mt-3\" style=\"font-size: 1.3em;\"\r\n            >Pelo menos um dos tipos de texto deve estar\r\n            habilitado!</v-card-text\r\n          >\r\n          <v-divider></v-divider>\r\n          <v-card-actions>\r\n            <v-spacer></v-spacer>\r\n            <v-btn color=\"primary\" text @click=\"dialog_alert = false\">Ok</v-btn>\r\n          </v-card-actions>\r\n        </v-card>\r\n      </v-dialog>\r\n    </div>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\n  import Api from \"@/services/Api\";\r\n\r\n  export default {\r\n    name: \"textDialog\",\r\n    props: [\"optionCall\", \"type\", \"dialog\", \"mobilemedia\"],\r\n    data: () => ({\r\n      valid: true,\r\n      dialog_alert: false,\r\n      switchTextFull: true,\r\n      switchTextShort: false,\r\n      infoClasse: \"\",\r\n      infoLevel: \"\",\r\n      infoLearning: \"\",\r\n      infoResume: \"\",\r\n      infoTextFull: \"\",\r\n      infoTextShort: \"\",\r\n      infoItemClasses: [\"Conceito\", \"Princípio\", \"Fato\"],\r\n      infoItemLevels: [\"0 - Inicial\", \"1 - Fácil\", \"2 - Médio\", \"3 - Difícil\"],\r\n      infoItemLearningStyles: [\"Visual\", \"Textual\"],\r\n    }),\r\n    watch: {\r\n      dialog: function() {\r\n        this.getMobileMedia();\r\n      },\r\n    },\r\n    mounted: function() {\r\n      this.getMobileMedia();\r\n      this.$refs.form.resetValidation();\r\n    },\r\n    methods: {\r\n      getMobileMedia() {\r\n        if (this.mobilemedia) {\r\n          var vm = this;\r\n          this.$nextTick(function() {\r\n            if (vm.mobilemedia.difficultyLevel !== null) {\r\n              vm.infoLevel = vm.infoItemLevels[vm.mobilemedia.difficultyLevel];\r\n            }\r\n            if (vm.mobilemedia.learningStyle !== null) {\r\n              vm.infoLearning =\r\n                vm.infoItemLearningStyles[vm.mobilemedia.learningStyle];\r\n            }\r\n            if (vm.mobilemedia.textfull !== null) {\r\n              vm.infoTextFull = vm.mobilemedia.textfull;\r\n              vm.switchTextFull = true;\r\n            } else {\r\n              vm.switchTextFull = false;\r\n            }\r\n            if (vm.mobilemedia.textshort !== null) {\r\n              vm.infoTextShort = vm.mobilemedia.textshort;\r\n              vm.switchTextShort = true;\r\n            } else {\r\n              vm.switchTextShort = false;\r\n            }\r\n            if (vm.mobilemedia.fk_informationitem) {\r\n              Api()\r\n                .patch(vm.mobilemedia.fk_informationitem, {})\r\n                .then(function(resposta) {\r\n                  vm.infoClasse =\r\n                    vm.infoItemClasses[\r\n                      resposta.data.fk_informationitemtype.split(\"/\")[4] - 1\r\n                    ];\r\n                });\r\n            }\r\n          });\r\n        }\r\n      },\r\n      async postOrPutMobileMedia() {\r\n        // var vm = this;\r\n        var auxinformationitem = {\r\n          auxinfo: `/informationitemtype/` + (this.infoClasse + 1) + \"/\",\r\n        };\r\n        var mobilemedia = {\r\n          label: \"\",\r\n          fk_idmediatype: \"/mediatype/4/\",\r\n          path: null,\r\n          namefile: null,\r\n          resolution: null,\r\n          description: null,\r\n          time: null,\r\n          urllink: null,\r\n        };\r\n\r\n        if (this.infoClasse == -1) {\r\n          this.infoClasse = 1;\r\n          auxinformationitem.auxinfo =\r\n            `/informationitemtype/` + this.infoClasse + \"/\";\r\n        }\r\n\r\n        if (this.switchTextFull) {\r\n          Object.assign(mobilemedia, {\r\n            textfull: this.infoTextFull,\r\n          });\r\n        } else {\r\n          Object.assign(mobilemedia, {\r\n            textfull: null,\r\n          });\r\n        }\r\n        if (this.switchTextShort) {\r\n          Object.assign(mobilemedia, {\r\n            textshort: this.infoTextShort,\r\n          });\r\n        } else {\r\n          Object.assign(mobilemedia, {\r\n            textshort: null,\r\n          });\r\n        }\r\n\r\n        if (this.infoLevel > -1) {\r\n          Object.assign(mobilemedia, {\r\n            difficultyLevel: this.infoLevel,\r\n          });\r\n        }\r\n\r\n        if (this.infoLearning > -1) {\r\n          Object.assign(mobilemedia, {\r\n            learningStyle: this.infoLearning,\r\n          });\r\n        }\r\n\r\n        if (this.type === \"conceito\") {\r\n          var iteminfo = {\r\n            nameinformationitem: \"text_\" + mobilemedia.textshort,\r\n            fk_informationitemtype: auxinformationitem.auxinfo,\r\n            fk_idconcept: this.optionCall.url,\r\n          };\r\n        } else if (this.type === \"dominio\") {\r\n          Object.assign(mobilemedia, {\r\n            fk_idknowledgedomain: this.optionCall.url,\r\n          });\r\n        } else if (this.type === \"modulo\") {\r\n          Object.assign(mobilemedia, {\r\n            fk_module: this.optionCall.url,\r\n          });\r\n        }\r\n\r\n        /* CÓDIGO PARA EDIÇÃO DO MOBILEMEDIA */\r\n        if (this.mobilemedia) {\r\n          var vm = this;\r\n          if (this.type === \"conceito\") {\r\n            await Api()\r\n              .put(vm.mobilemedia.fk_informationitem, iteminfo)\r\n              .then(async function() {\r\n                await Api()\r\n                  .put(vm.mobilemedia.url, mobilemedia)\r\n                  .then(async function(/*resposta*/) {});\r\n              });\r\n          } else if (this.type === \"dominio\" || this.type === \"modulo\") {\r\n            await Api()\r\n              .put(this.mobilemedia.url, mobilemedia)\r\n              .then(async function(/*resposta*/) {});\r\n          }\r\n        } else {\r\n          /* CÓDIGO PARA CRIAÇÃO DO MOBILEMEDIA */\r\n          if (this.type === \"conceito\") {\r\n            /* CÓDIGO PARA CRIAÇÃO DO MOBILEMEDIA COM ITEMINFO*/\r\n            await Api()\r\n              .post(`/informationitem/`, iteminfo)\r\n              .then(async function(resposta) {\r\n                Object.assign(mobilemedia, {\r\n                 