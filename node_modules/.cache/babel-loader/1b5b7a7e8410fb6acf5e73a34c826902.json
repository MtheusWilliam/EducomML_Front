AxBA;AAyBA,MAAA,wBAAA,EAAA,CACA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,CAAA,IAAA,mDAAA;AAAA,OADA,EAEA,UAAA,CAAA;AAAA,eACA,CAAA,IAAA,CAAA,CAAA,MAAA,IAAA,GAAA,IACA,mDAFA;AAAA,OAFA,CAzBA;AA+BA,MAAA,QAAA,EAAA,EA/BA;AAgCA,MAAA,aAAA,EAAA,CACA,UAAA,CAAA;AAAA,eACA,CAAA,CAAA,MAAA,IAAA,GAAA,IACA,wDAFA;AAAA,OADA,CAhCA;AAqCA,MAAA,QAAA,EAAA,EArCA;AAsCA,MAAA,MAAA,EAAA,IAtCA;AAuCA,MAAA,KAAA,EAAA,CACA;AAAA,QAAA,KAAA,EAAA,MAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OADA,EAEA;AAAA,QAAA,KAAA,EAAA,YAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAFA,EAGA;AAAA,QAAA,KAAA,EAAA,OAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAHA,CAvCA;AA4CA,MAAA,IAAA,EAAA;AA5CA,KAAA;AAAA,GALA;AAmDA,EAAA,OAAA,EAAA,mBAAA;AACA,SAAA,WAAA;AACA,GArDA;AAsDA,EAAA,QAAA,EAAA;AACA,IAAA,eADA,6BACA;AAAA;;AACA,UAAA,IAAA,GAAA,KAAA,QAAA,CAAA,MAAA,CAAA,UAAA,MAAA,EAAA;AACA,eACA,MAAA,CAAA,mBAAA,CACA,WADA,GAEA,OAFA,CAEA,KAAA,CAAA,MAAA,CAAA,WAAA,EAFA,IAEA,CAAA,CAHA;AAKA,OANA,CAAA;AAOA,aAAA,IAAA;AACA;AAVA,GAtDA;AAkEA,EAAA,OAAA,EAAA;AACA,IAAA,WADA,yBACA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,aAAA,GAAA,IAAA;AACA,gBAAA,EAFA,GAEA,MAFA;AAAA;AAAA,uBAIA,GAAA,GAAA,IAAA,CAAA,SAAA,EAAA;AACA,kBAAA,QAAA,EAAA,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA;AADA,iBAAA,CAJA;;AAAA;AAIA,gBAAA,QAJA;AAAA;AAAA,uBAQA,GAAA,GACA,GADA,CACA,QAAA,CAAA,IAAA,CAAA,GADA,EAEA,IAFA,CAEA,UAAA,SAAA,EAAA;AACA,kBAAA,EAAA,CAAA,QAAA,GAAA,SAAA,CAAA,IAAA,CAAA,gBAAA;AACA,kBAAA,EAAA,CAAA,QAAA,GAAA,SAAA,CAAA,IAAA,CAAA,QAAA;AACA,kBAAA,EAAA,CAAA,aAAA,GAAA,KAAA;AACA,kBAAA,EAAA,CAAA,mBAAA;AACA,iBAPA,CARA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgBA,KAjBA;AAkBA,IAAA,UAlBA,sBAkBA,QAlBA,EAkBA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,oBAAA,EAAA,QAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,QADA;AAEA,QAAA,MAAA,EAAA;AACA,UAAA,QAAA,EAAA;AADA;AAFA,OAAA;AAMA,KA1BA;AA2BA,IAAA,cA3BA,0BA2BA,SA3BA,EA2BA;AACA,WAAA,YAAA,GAAA,SAAA;AACA,WAAA,WAAA,GAAA,IAAA;AACA,KA9BA;AA+BA,IAAA,aA/BA,2BA+BA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,EADA,GACA,MADA;AAAA;AAAA,uBAGA,GAAA,GACA,MADA,CACA,MAAA,CAAA,YADA,EAEA,IAFA,CAEA,YAAA;AACA,kBAAA,EAAA,CAAA,WAAA;AACA,kBAAA,EAAA,CAAA,WAAA,GAAA,KAAA;AACA,kBAAA,EAAA,CAAA,YAAA,GAAA,EAAA;AACA,iBANA,CAHA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAUA,KAzCA;AA0CA,IAAA,WA1CA,yBA0CA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,EADA,GACA,MADA;AAAA;AAAA,uBAGA,GAAA,GACA,IADA,CACA,SADA,EACA;AACA,kBAAA,QAAA,EAAA,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA;AADA,iBADA,EAIA,IAJA;AAAA,qFAIA,kBAAA,QAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mCACA,GAAA,GACA,IADA,sBACA;AACA,8BAAA,mBAAA,EAAA,MAAA,CAAA,mBADA;AAEA,8BAAA,QAAA,EAAA,MAAA,CAAA,QAFA;AAGA,8BAAA,WAAA,EAAA,MAAA,CAAA,WAHA;AAIA,8BAAA,SAAA,EAAA,QAAA,CAAA,IAAA,CAAA;AAJA,6BADA,EAOA,IAPA,CAOA,UAAA,QAAA,EAAA;AACA,8BAAA,EAAA,CAAA,QAAA,GAAA,QAAA,CAAA,IAAA,CAAA,iBAAA;AACA,8BAAA,EAAA,CAAA,MAAA,CAAA,QAAA,CACA,oBADA,EAEA,QAAA,CAAA,IAAA,CAAA,iBAFA;AAIA,8BAAA,EAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,gCAAA,IAAA,EAAA,QADA;AAEA,gCAAA,MAAA,EAAA;AACA,kCAAA,QAAA,EAAA,QAAA,CAAA,IAAA,CAAA;AADA;AAFA,+BAAA;AAMA,6BAnBA,CADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mBAJA;;AAAA;AAAA;AAAA;AAAA,oBAHA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA6BA,KAvEA;AAwEA,IAAA,mBAxEA,iCAwEA;AACA,UAAA,KAAA,QAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,aAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,QAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,cAAA,aAAA,GAAA,KAAA,iBAAA,CACA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,MAAA,KAAA,KAAA,iBAAA,CAAA,MAAA,CADA,CAAA;AAGA,eAAA,YAAA,CAAA,IAAA,CAAA,aAAA;AACA;AACA;AACA,KAjFA;AAkFA,IAAA,QAlFA,sBAkFA;AACA,UAAA,KAAA,KAAA,CAAA,IAAA,CAAA,QAAA,EAAA,EAAA;AACA,aAAA,KAAA,CAAA,IAAA,CAAA,QAAA;AACA,aAAA,WAAA;AACA;AACA,KAvFA;AAwFA,IAAA,KAxFA,mBAwFA;AACA,WAAA,MAAA,GAAA,KAAA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,KAAA;AACA,KA3FA;AA4FA,IAAA,eA5FA,6BA4FA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,eAAA;AACA,KA9FA;AA+FA,IAAA,SA/FA,qBA+FA,CA/FA,EA+FA;AACA,WAAA,MAAA,GAAA,CAAA;AACA;AAjGA;AAlEA,CAAA","sourcesContent":["<template>\r\n  <v-row>\r\n    <!-- MENU DO USUÁRIO -->\r\n    <v-navigation-drawer\r\n      class=\"ml-6 mt-3 mr-3\"\r\n      v-model=\"drawer\"\r\n      :mini-variant.sync=\"mini\"\r\n      permanent\r\n      color=\"#B19114\"\r\n      dark\r\n    >\r\n      <ListDomain :domains=\"filteredDomains\" @emitDomains=\"setSearch\" />\r\n    </v-navigation-drawer>\r\n    <!-- FORM DO DOMÍNIO -->\r\n    <v-col class=\"mr-6\">\r\n      <v-row>\r\n        <v-app-bar color=\"#B19114\" style=\"width:100%;\">\r\n          <v-toolbar-title style=\"font-size:1.4em; color:white;\">Domínios</v-toolbar-title>\r\n        </v-app-bar>\r\n        <v-container>\r\n          <v-row v-if=\"filteredDomains.length > 0\" class=\"mb-6\">\r\n            <v-spacer></v-spacer>\r\n            <v-col cols=\"4\" v-for=\"(dominio, i) in filteredDomains\" :key=\"i\">\r\n              <v-card height=\"350px\">\r\n                <v-img\r\n                  class=\"white--text align-end\"\r\n                  height=\"200px\"\r\n                  :style=\"'background-color: ' + domainColors[i]\"\r\n                >\r\n                  <v-card-title>{{ dominio.nameknowledgedomain }}</v-card-title>\r\n                </v-img>\r\n                <v-row style=\"height: 100px;\">\r\n                  <v-card-subtitle class=\"pb-0 ml-3\" style=\"color:black;\">{{ dominio.subtitle }}</v-card-subtitle>\r\n                </v-row>\r\n                <v-card-actions>\r\n                  <v-btn class=\"ml-2\" color=\"red\" dark @click=\"getAlertDelete(dominio.url)\">\r\n                    <v-icon>mdi-delete</v-icon>\r\n                  </v-btn>\r\n                  <v-spacer></v-spacer>\r\n                  <v-btn\r\n                    class=\"mr-2\"\r\n                    color=\"primary\"\r\n                    @click=\"putDominio(dominio.idknowledgedomain)\"\r\n                  >\r\n                    Editar\r\n                    <v-icon class=\"ml-2\" small>mdi-pencil</v-icon>\r\n                  </v-btn>\r\n                </v-card-actions>\r\n              </v-card>\r\n            </v-col>\r\n            <v-spacer></v-spacer>\r\n          </v-row>\r\n          <v-row v-else>\r\n            <v-spacer></v-spacer>\r\n            <h3 class=\"pt-8 pb-12\">Você não tem nenhum domínio atualmente...</h3>\r\n            <v-spacer></v-spacer>\r\n          </v-row>\r\n\r\n          <v-row>\r\n            <v-dialog v-model=\"dialog\" persistent max-width=\"600px\">\r\n              <template v-slot:activator=\"{ on }\">\r\n                <v-btn\r\n                  class=\"mx-auto\"\r\n                  width=\"50%\"\r\n                  x-large\r\n                  color=\"primary\"\r\n                  dark\r\n                  v-on=\"on\"\r\n                >Criar Novo Domínio</v-btn>\r\n              </template>\r\n\r\n              <v-card>\r\n                <v-card-title style=\"background-color:#63B0B0; color:white;\">\r\n                  <span class=\"headline\">\r\n                    <p>Defina o domínio do conhecimento a ser modelado</p>\r\n                  </span>\r\n                </v-card-title>\r\n                <v-card-text>\r\n                  <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\r\n                    <v-text-field\r\n                      v-model=\"nameknowledgedomain\"\r\n                      :counter=\"100\"\r\n                      :rules=\"nameknowledgedomainRules\"\r\n                      label=\"Dominío modelado\"\r\n                      required\r\n                    ></v-text-field>\r\n\r\n                    <v-text-field\r\n                      v-model=\"subtitle\"\r\n                      :rules=\"subtitleRules\"\r\n                      label=\"Subítulo para o conteúdo modelado\"\r\n                    ></v-text-field>\r\n                  </v-form>\r\n                </v-card-text>\r\n                <v-card-actions>\r\n                  <v-spacer></v-spacer>\r\n                  <v-btn color=\"red\" height=\"49\" dark large @click=\"reset\">\r\n                    Cancelar\r\n                    <v-icon dark right>mdi-close</v-icon>\r\n                  </v-btn>\r\n                  <v-btn color=\"success\" height=\"49\" dark large @click=\"validate\">\r\n                    Salvar\r\n                    <v-icon dark right>mdi-content-save</v-icon>\r\n                  </v-btn>\r\n                </v-card-actions>\r\n              </v-card>\r\n            </v-dialog>\r\n          </v-row>\r\n        </v-container>\r\n      </v-row>\r\n    </v-col>\r\n    <div class=\"text-center\">\r\n      <v-dialog v-model=\"alertDelete\" width=\"500\" persistent=\"persistent\">\r\n        <v-card>\r\n          <v-card-title class=\"headline red\" primary-title style=\"color:white;\">ALERTA!</v-card-title>\r\n          <v-card-text\r\n            class=\"mt-3\"\r\n            style=\"font-size: 1.3em;\"\r\n          >Tem certeza que deseja apagar esse domínio?</v-card-text>\r\n          <v-divider></v-divider>\r\n          <v-card-actions>\r\n            <v-btn color=\"primary\" @click=\"deleteDominio(auxUrlDomain)\">Sim</v-btn>\r\n            <v-spacer></v-spacer>\r\n            <v-btn\r\n              color=\"red\"\r\n              dark\r\n              @click=\"\r\n                auxUrlDomain = '';\r\n                alertDelete = false;\r\n              \"\r\n            >Não</v-btn>\r\n          </v-card-actions>\r\n        </v-card>\r\n      </v-dialog>\r\n    </div>\r\n    <div class=\"text-center\">\r\n      <v-dialog v-model=\"dialogLoading\" max-width=\"290\" persistent=\"persistent\">\r\n        <v-card color=\"primary\" dark>\r\n          <v-card-text style=\"color:white;\">\r\n            <v-row class=\"pt-2 pb-3\">\r\n              <br />\r\n              <v-spacer></v-spacer>\r\n              <span style=\"font-size: 1.3em; color:white;\">Carregando seus domínios</span>\r\n              <v-spacer></v-spacer>\r\n            </v-row>\r\n            <v-row>\r\n              <v-spacer></v-spacer>\r\n              <v-progress-circular indeterminate color=\"white\" class=\"mb-0\"></v-progress-circular>\r\n              <v-spacer></v-spacer>\r\n            </v-row>\r\n          </v-card-text>\r\n        </v-card>\r\n      </v-dialog>\r\n    </div>\r\n  </v-row>\r\n</template>\r\n\r\n<script>\r\nimport Api from \"@/services/Api\";\r\nimport ListDomain from \"@/components/ListDomain\";\r\n\r\nexport default {\r\n  name: \"UserHome\",\r\n  components: {\r\n    ListDomain,\r\n  },\r\n  data: () => ({\r\n    search: \"\",\r\n    valid: true,\r\n    dialog: false,\r\n    dialogLoading: false,\r\n    lastversion: \"versao_teste\",\r\n    alertDelete: false,\r\n    auxUrlDomain: \"\",\r\n    idDomain: 0,\r\n    domainColors: [],\r\n    cardColorsPalette: [\r\n      \"#DEB887\",\r\n      \"#D2B48C\",\r\n      \"#BC8F8F\",\r\n      \"#F4A460\",\r\n      \"#DAA520\",\r\n      \"#CD853F\",\r\n      \"#D2691E\",\r\n      \"#8B4513\",\r\n      \"#A0522D\",\r\n      \"#A52A2A\",\r\n      \"#800000\",\r\n    ],\r\n    userName: \"\",\r\n    nameknowledgedomain: \"\",\r\n    nameknowledgedomainRules: [\r\n      (v) => !!v || \"É necessário descrever o nome do domínio modelado\",\r\n      (v) =>\r\n        (v && v.length <= 100) ||\r\n        \"Nome do domínio deve ter no máximo 100 caracteres\",\r\n    ],\r\n    subtitle: \"\",\r\n    subtitleRules: [\r\n      (v) =>\r\n        v.length <= 100 ||\r\n        \"Subtítulo do domínio deve ter no máximo 100 caracteres\",\r\n    ],\r\n    dominios: [],\r\n    drawer: true,\r\n    items: [\r\n      { title: \"Home\", icon: \"mdi-home-city\" },\r\n      { title: \"My Account\", icon: \"mdi-account\" },\r\n      { title: \"Users\", icon: \"mdi-account-group-outline\" },\r\n    ],\r\n    mini: false,\r\n  }),\r\n  mounted: function () {\r\n    this.getDominios();\r\n  },\r\n  computed: {\r\n    filteredDomains() {\r\n      var temp = this.dominios.filter((domain) => {\r\n        return (\r\n          domain.nameknowledgedomain\r\n            .toLowerCase()\r\n            .indexOf(this.search.toLowerCase()) > -1\r\n        );\r\n      });\r\n      return temp;\r\n    },\r\n  },\r\n  methods: {\r\n    async getDominios() {\r\n      this.dialogLoading = true;\r\n      var vm = this;\r\n\r\n      var response = await Api().post(\"userId/\", {\r\n        username: this.$store.state.username,\r\n      });\r\n\r\n      await Api()\r\n        .get(response.data.url)\r\n        .then((response2) => {\r\n          vm.dominios = response2.data.knowledgedomains;\r\n          vm.userName = response2.data.username;\r\n          vm.dialogLoading = false;\r\n          vm.getDomainCardColors();\r\n        });\r\n    },\r\n    putDominio(idDomain) {\r\n      this.$store.dispatch(\"getActualKnowledge\", idDomain);\r\n      this.$router.push({\r\n        name: \"create\",\r\n        params: {\r\n          idDomain: idDomain,\r\n        },\r\n      });\r\n    },\r\n    getAlertDelete(urlDomain) {\r\n      this.auxUrlDomain = urlDomain;\r\n      this.alertDelete = true;\r\n    },\r\n    async deleteDominio() {\r\n      var vm = this;\r\n\r\n      await Api()\r\n        .delete(this.auxUrlDomain)\r\n        .then(function () {\r\n          vm.getDominios();\r\n          vm.alertDelete = false;\r\n          vm.auxUrlDomain = \"\";\r\n        });\r\n    },\r\n    async postDominio() {\r\n      var vm = this;\r\n\r\n      await Api()\r\n        .post(\"userId/\", {\r\n          username: this.$store.state.username,\r\n        })\r\n        .then(async (response) => {\r\n          await Api()\r\n            .post(`/knowledgedomain/`, {\r\n              nameknowledgedomain: this.nameknowledgedomain,\r\n              subtitle: this.subtitle,\r\n              lastversion: this.lastversion,\r\n              fk_iduser: response.data.url,\r\n            })\r\n            .then(function (resposta) {\r\n              vm.idDomain = resposta.data.idknowledgedomain;\r\n              vm.$store.dispatch(\r\n                \"getActualKnowledge\",\r\n                resposta.data.idknowledgedomain\r\n              );\r\n              vm.$router.push({\r\n                name: \"create\",\r\n                params: {\r\n                  idDomain: resposta.data.idknowledgedomain,\r\n                },\r\n              });\r\n            });\r\n        });\r\n    },\r\n    getDomainCardColors() {\r\n      if (this.dominios.length > 0) {\r\n        for (var i = 0; i < this.dominios.length; i++) {\r\n          var randomElement = this.cardColorsPalette[\r\n            Math.floor(Math.random() * this.cardColorsPalette.length)\r\n          ];\r\n          this.domainColors.push(randomElement);\r\n        }\r\n      }\r\n    },\r\n    validate() {\r\n      if (this.$refs.form.validate()) {\r\n        this.$refs.form.validate();\r\n        this.postDominio();\r\n      }\r\n    },\r\n    reset() {\r\n      this.dialog = false;\r\n      this.$refs.form.reset();\r\n    },\r\n    resetValidation() {\r\n      this.$refs.form.resetValidation();\r\n    },\r\n    setSearch(s) {\r\n      this.search = s;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"],"sourceRoot":"src/components"}]}