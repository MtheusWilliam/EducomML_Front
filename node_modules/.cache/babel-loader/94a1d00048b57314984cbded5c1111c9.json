A,CAAA,MAAA,CAAA,iBAAA,KAAA,GAAA,EAAA;AACA,WAAA,YAAA,GAAA,cAAA;AACA,WAAA,YAAA,GAAA,qBAAA;AACA,WAAA,OAAA,GACA,8EADA;AAEA,WAAA,aAAA,GAAA,IAAA;AACA;AACA,GAhCA;AAiCA,EAAA,OAAA,EAAA;AACA,IAAA,KADA,mBACA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBACA,KAAA,CAAA,aADA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAEA,KAAA,CAAA,MAAA,CAAA,MAAA,CAAA,YAAA,EAAA;AACA,kBAAA,MAAA,EAAA,IADA;AAEA,kBAAA,OAAA,EACA;AAHA,iBAAA,CAFA;;AAAA;AAAA;AAAA,uBAOA,GAAA,GACA,IADA,CAEA,kBAFA,EAGA;AACA,kBAAA,KAAA,EAAA,KAAA,CAAA;AADA,iBAHA,EAMA;AACA,kBAAA,IAAA,EAAA;AACA,oBAAA,QAAA,EAAA,OADA;AAEA,oBAAA,QAAA,EAAA;AAFA;AADA,iBANA,EAaA,IAbA,CAaA,UAAA,QAAA,EAAA;AACA,sBAAA,QAAA,CAAA,IAAA,CAAA,MAAA,EAAA;AACA,oBAAA,KAAA,CAAA,YAAA,GAAA,gBAAA;AACA,oBAAA,KAAA,CAAA,YAAA,GAAA,gBAAA;AACA,mBAHA,MAGA;AACA,oBAAA,KAAA,CAAA,YAAA,GAAA,+BAAA;AACA,oBAAA,KAAA,CAAA,YAAA,GAAA,cAAA;AACA;;AACA,kBAAA,KAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA,kBAAA,KAAA,CAAA,aAAA,GAAA,IAAA;AACA,iBAvBA,CAPA;;AAAA;AAAA;AAAA,uBA+BA,KAAA,CAAA,MAAA,CAAA,MAAA,CAAA,YAAA,EAAA;AACA,kBAAA,MAAA,EAAA,KADA;AAEA,kBAAA,OAAA,EAAA;AAFA,iBAAA,CA/BA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA,uBAoCA,KAAA,CAAA,MAAA,CACA,QADA,CACA,aADA,EACA;AACA,kBAAA,QAAA,EAAA,KAAA,CAAA,QADA;AAEA,kBAAA,QAAA,EAAA,KAAA,CAAA;AAFA,iBADA,EAKA,IALA,CAKA,YAAA;AACA,sBAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,GAAA,EAAA;AACA,oBAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,sBAAA,IAAA,EAAA;AAAA,qBAAA;AACA,mBAFA,MAEA;AACA,oBAAA,KAAA,CAAA,YAAA,GAAA,cAAA;AACA,oBAAA,KAAA,CAAA,YAAA,GAAA,gBAAA;AACA,oBAAA,KAAA,CAAA,OAAA,GACA,kFADA;AAEA,oBAAA,KAAA,CAAA,aAAA,GAAA,IAAA;AACA;AACA,iBAfA,CApCA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAqDA,KAtDA;AAuDA,IAAA,cAvDA,4BAuDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,kBAAA,IAAA,EAAA;AADA,iBAAA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA;AA3DA;AAjCA,CAAA","sourcesContent":["<template>\r\n  <v-card class=\"elevation-12 mx-auto\" style=\"position:relative;top:20vh;\" width=\"50vw\">\r\n    <v-toolbar color=\"black\" flat>\r\n      <v-toolbar-title style=\"color:#FFCC00;\">Faça seu login no Educom.ML</v-toolbar-title>\r\n      <v-spacer></v-spacer>\r\n      <v-img class=\"mt-1\" src=\"../assets/logo.png\" style=\"width: 20px;\" max-width=\"200\"></v-img>\r\n      <v-tooltip bottom>\r\n        <span>Source</span>\r\n      </v-tooltip>\r\n    </v-toolbar>\r\n    <v-card-text>\r\n      <v-form style=\"margin-bottom: -20px;\">\r\n        <v-text-field\r\n          v-if=\"!resetPassword\"\r\n          v-model=\"username\"\r\n          label=\"Username\"\r\n          name=\"login\"\r\n          prepend-icon=\"mdi-account\"\r\n          type=\"text\"\r\n          required\r\n        ></v-text-field>\r\n\r\n        <v-text-field\r\n          v-if=\"!resetPassword\"\r\n          id=\"password\"\r\n          label=\"Senha\"\r\n          name=\"password\"\r\n          v-model=\"password\"\r\n          prepend-icon=\"mdi-lock\"\r\n          type=\"password\"\r\n          required\r\n        ></v-text-field>\r\n\r\n        <v-text-field\r\n          v-if=\"resetPassword\"\r\n          id=\"email\"\r\n          label=\"E-mail\"\r\n          name=\"email\"\r\n          :rules=\"emailRules\"\r\n          v-model=\"email\"\r\n          prepend-icon=\"mdi-email\"\r\n          type=\"email\"\r\n          required\r\n        ></v-text-field>\r\n      </v-form>\r\n    </v-card-text>\r\n    <v-card-actions>\r\n      <a @click=\"resetPassword = !resetPassword\" class=\"ml-5\">\r\n        <span v-if=\"!resetPassword\">Esqueceu a senha?</span>\r\n        <span v-else>Voltar</span>\r\n      </a>\r\n      <v-spacer></v-spacer>\r\n      <a @click=\"redirectSignUp()\" class=\"ml-5\">\r\n        <span>Não possui uma conta? Faça seu cadastro</span>\r\n      </a>\r\n      <v-spacer></v-spacer>\r\n      <v-btn color=\"primary\" @click=\"login\" large class=\"mr-4\">\r\n        <span v-if=\"!resetPassword\">Login</span>\r\n        <span v-else>Enviar Email</span>\r\n      </v-btn>\r\n    </v-card-actions>\r\n    <div class=\"text-center\">\r\n      <v-dialog v-model=\"dialogMessage\" width=\"500\">\r\n        <v-card>\r\n          <v-card-title :class=\"messageClass\" primary-title style=\"color:white;\">{{ messageTitle }}</v-card-title>\r\n          <v-card-text class=\"mt-3\" style=\"font-size: 1.3em;\">{{ message }}</v-card-text>\r\n          <v-divider></v-divider>\r\n          <v-card-actions>\r\n            <v-spacer></v-spacer>\r\n            <v-btn color=\"primary\" text @click=\"dialogMessage = false\">Ok</v-btn>\r\n          </v-card-actions>\r\n        </v-card>\r\n      </v-dialog>\r\n    </div>\r\n  </v-card>\r\n</template>\r\n\r\n<script>\r\nimport Api from \"@/services/Api\";\r\nexport default {\r\n  name: \"Login\",\r\n  data() {\r\n    return {\r\n      username: \"\",\r\n      password: \"\",\r\n      email: \"\",\r\n      emailRules: [\r\n        (v) => !!v || \"É necessário inserir seu e-mail\",\r\n        (v) => /.+@.+\\..+/.test(v) || \"É necessário inserir um e-mail válido\",\r\n      ],\r\n      resetPassword: false,\r\n      dialogMessage: false,\r\n      messageClass: \"\",\r\n      messageTitle: \"\",\r\n      message: \"\",\r\n    };\r\n  },\r\n  mounted() {\r\n    if (this.$route.params.emailconfirmation === \"1\") {\r\n      this.messageClass = \"headline green\";\r\n      this.messageTitle = \"Email confirmado com sucesso\";\r\n      this.message =\r\n        \"Seu cadastro foi confirmado! Faça login para começar a usar a plataforma EducomML.\";\r\n      this.dialogMessage = true;\r\n    } else if (this.$route.params.emailconfirmation === \"0\") {\r\n      this.messageClass = \"headline red\";\r\n      this.messageTitle = \"Erro na confirmação\";\r\n      this.message =\r\n        \"Houve um problema na confirmação da sua conta, realize o cadastro novamente.\";\r\n      this.dialogMessage = true;\r\n    }\r\n  },\r\n  methods: {\r\n    async login() {\r\n      if (this.resetPassword) {\r\n        await this.$store.commit(\"setLoading\", {\r\n          dialog: true,\r\n          message:\r\n            \"Estamos enviando um email de redefinição de senha para você...\",\r\n        });\r\n        await Api()\r\n          .post(\r\n            \"/reset-password/\",\r\n            {\r\n              email: this.email,\r\n            },\r\n            {\r\n              auth: {\r\n                username: \"admin\",\r\n                password: \"admin\",\r\n              },\r\n            }\r\n          )\r\n          .then((response) => {\r\n            if (response.data.status) {\r\n              this.messageTitle = \"Email enviado!\";\r\n              this.messageClass = \"headline green\";\r\n            } else {\r\n              this.messageTitle = \"Erro na redefinição de senha!\";\r\n              this.messageClass = \"headline red\";\r\n            }\r\n            this.message = response.data.message;\r\n            this.dialogMessage = true;\r\n          });\r\n        await this.$store.commit(\"setLoading\", {\r\n          dialog: false,\r\n          message: \"\",\r\n        });\r\n      } else {\r\n        await this.$store\r\n          .dispatch(\"obtainToken\", {\r\n            username: this.username,\r\n            password: this.password,\r\n          })\r\n          .then(() => {\r\n            if (this.$store.state.jwt) {\r\n              this.$router.push({ path: \"/home\" });\r\n            } else {\r\n              this.messageClass = \"headline red\";\r\n              this.messageTitle = \"Erro no Login!\";\r\n              this.message =\r\n                \"Verifique login e senha inseridos e se o cadastro da sua conta já foi confirmada\";\r\n              this.dialogMessage = true;\r\n            }\r\n          });\r\n      }\r\n    },\r\n    async redirectSignUp() {\r\n      this.$router.push({\r\n        name: \"signup\",\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components"}]}