CAAA,IAAA,CAAA,uCAAA,EAAA;AACA,kBAAA,KAAA,EAAA,KAAA,CAAA;AADA,iBAAA,EAEA;AACA,kBAAA,OAAA,EAAA;AACA,mDAAA;AADA;AADA,iBAFA,EAMA,IANA,CAMA,UAAA,QAAA,EAAA;AACA,kBAAA,KAAA,CAAA,YAAA,GAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA,iBARA,CAFA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA,uBAaA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,aAAA,EAAA;AACA,kBAAA,QAAA,EAAA,KAAA,CAAA,QADA;AAEA,kBAAA,QAAA,EAAA,KAAA,CAAA;AAFA,iBAAA,CAbA;;AAAA;AAAA;AAAA,uBAiBA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,kBAAA,IAAA,EAAA;AADA,iBAAA,CAjBA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAqBA,gBAAA,KAAA,CAAA,YAAA,GAAA,4DAAA;;AArBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAyBA;AA1BA;AAfA,CAAA","sourcesContent":["<template>\r\n  <v-card class=\"elevation-12\">\r\n    <v-toolbar color=\"black\" flat>\r\n      <v-toolbar-title style=\"color:#FFCC00;\">Faça seu login no Educom.ML</v-toolbar-title>\r\n      <v-spacer></v-spacer>\r\n      <v-img class=\"mt-1\" src=\"../assets/logo.png\" style=\"width: 20px;\"></v-img>\r\n      <v-tooltip bottom>\r\n        <span>Source</span>\r\n      </v-tooltip>\r\n    </v-toolbar>\r\n    <v-card-text>\r\n      <v-form style=\"margin-bottom: -20px;\">\r\n        <v-text-field\r\n          v-if=\"!resetPassword\"\r\n          v-model=\"username\"\r\n          label=\"Username\"\r\n          name=\"login\"\r\n          prepend-icon=\"mdi-account\"\r\n          type=\"text\"\r\n          required\r\n        ></v-text-field>\r\n\r\n        <v-text-field\r\n          v-if=\"!resetPassword\"\r\n          id=\"password\"\r\n          label=\"Senha\"\r\n          name=\"password\"\r\n          v-model=\"password\"\r\n          prepend-icon=\"mdi-lock\"\r\n          type=\"password\"\r\n          required\r\n        ></v-text-field>\r\n\r\n        <v-text-field\r\n          v-if=\"resetPassword\"\r\n          id=\"email\"\r\n          label=\"E-mail\"\r\n          name=\"email\"\r\n          :rules=\"emailRules\"\r\n          v-model=\"email\"\r\n          prepend-icon=\"mdi-email\"\r\n          type=\"email\"\r\n          required\r\n        ></v-text-field>\r\n\r\n      </v-form>\r\n    </v-card-text>\r\n    <v-card-actions>\r\n      <a @click=\"resetPassword = !resetPassword;messageError=''\" class=\"ml-5\"><span v-if=\"!resetPassword\">Esqueceu a senha?</span><span v-else>Voltar</span></a>\r\n      <v-spacer></v-spacer>\r\n      {{ messageError }}\r\n      <v-btn color=\"primary\" @click=\"login\" large class=\"mr-4\"><span v-if=\"!resetPassword\">Login</span><span v-else>Enviar Email</span></v-btn>\r\n    </v-card-actions>\r\n    <!--\r\n    ---\r\n    <v-card-text class=\"mt-3\">\r\n      <p style=\"text-align: center; font-size: 1.2em;\">\r\n        Não tem conta?\r\n        <a href=\"#\" style=\"text-decoration: none;\">\r\n          <strong>CADASTRE-SE</strong>\r\n        </a>.\r\n      </p>\r\n    </v-card-text>\r\n    -->\r\n  </v-card>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Login\",\r\n  data() {\r\n    return {\r\n      username: \"\",\r\n      password: \"\",\r\n      email: \"\",\r\n      emailRules: [\r\n        v => !!v || \"É necessário inserir seu e-mail\",\r\n        v => /.+@.+\\..+/.test(v) || \"É necessário inserir um e-mail válido\"\r\n      ],\r\n      resetPassword: false,\r\n      messageError: \"\"\r\n    };\r\n  },\r\n  methods: {\r\n    async login() {\r\n      if(this.resetPassword){\r\n        await this.axios.post('http://localhost:8000/reset-password/',{\r\n          email: this.email,\r\n        },{\r\n          headers: {\r\n            'Access-Control-Allow-Origin': '*',\r\n            }\r\n          }).then(response => {\r\n          this.messageError = response.data.message;\r\n        });\r\n      }else{\r\n        try {\r\n        await this.$store.dispatch(\"obtainToken\", {\r\n          username: this.username,\r\n          password: this.password\r\n        });\r\n        await this.$router.push({\r\n          name: \"home\"\r\n        });\r\n      } catch (err) {\r\n        this.messageError = \"Você ainda nao verificou o seu email ou username já existe\";\r\n      }\r\n      }\r\n      \r\n    }\r\n  }\r\n};\r\n</script>"],"sourceRoot":"src/components"}]}