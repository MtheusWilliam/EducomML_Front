A;AAoBA,EAAA,OApBA,qBAoBA;AACA;;;AAIA,QAAA,EAAA,GAAA,IAAA;AACA,IAAA,UAAA,CAAA,YAAA;AACA,MAAA,EAAA,CAAA,kBAAA,CAAA,EAAA,CAAA,OAAA;AACA,KAFA,EAEA,IAFA,CAAA;AAIA,GA9BA;AA+BA,EAAA,QAAA,EAAA;AACA,IAAA,gBAAA,EAAA,4BAAA;AACA,aAAA,KAAA,UAAA;AACA;AAHA,GA/BA;AAoCA,EAAA,OAAA,EAAA;AACA,IAAA,QADA,sBACA;AACA,UAAA,KAAA,UAAA,IAAA,KAAA,kBAAA,IAAA,KAAA,iBAAA,EAAA;AACA,aAAA,UAAA;AACA,aAAA,cAAA,GAAA,KAAA;AACA;AACA,KANA;AAOA,IAAA,eAPA,6BAOA;AACA,WACA,KADA,CAEA,IAFA,CAGA,eAHA;AAIA,KAZA;AAaA,IAAA,KAbA,mBAaA;AACA,WAAA,cAAA,GAAA,KAAA;AACA,WAAA,kBAAA;AACA,KAhBA;AAiBA,IAAA,kBAjBA,8BAiBA,YAjBA,EAiBA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,YAAA;AACA,WAAA,YAAA,GAAA,YAAA;AACA,WAAA,OAAA,GAAA,KAAA,YAAA,CAAA,OAAA;AACA,KArBA;AAsBA,IAAA,UAtBA,wBAsBA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,UAAA,SAAA,GAAA,MAAA,CAAA,GAAA,CAAA,WAAA,CAAA;AACA,UAAA,OAAA,GAAA;AACA,uBAAA;AADA,OAAA;AAGA,MAAA,KAAA,CACA,KADA,CAEA,2CAAA,KAAA,YAAA,CAAA,iBAAA,GACA,GAHA,EAIA;AACA,QAAA,OAAA,EAAA;AADA,OAJA,EAOA;AACA,QAAA,IAAA,EAAA;AACA,UAAA,QAAA,EAAA,OADA;AAEA,UAAA,QAAA,EAAA;AAFA;AADA,OAPA,EAcA,IAdA,CAcA,UAAA,QAAA,EAAA;AACA,QAAA,EAAA,CAAA,kBAAA,CAAA,QAAA,CAAA,IAAA;AACA,OAhBA;AAiBA,KA7CA;AA8CA,IAAA,oBA9CA,gCA8CA,KA9CA,EA8CA;AACA,UAAA,EAAA,GAAA,IAAA;;AACA,UAAA,KAAA,KAAA,MAAA,EAAA;AACA,QAAA,UAAA,CAAA,YAAA;AACA,UAAA,EAAA,CAAA,UAAA;AACA,SAFA,EAEA,IAFA,CAAA;AAGA,aAAA,aAAA,GAAA,KAAA;AACA,OALA,MAKA;AACA,aAAA,aAAA,GAAA,KAAA;AACA;AACA,KAxDA;AAyDA,IAAA,qBAzDA,iCAyDA,KAzDA,EAyDA;AACA,UAAA,KAAA,KAAA,MAAA,EAAA;AAEA,aAAA,cAAA,GAAA,CAAA,KAAA,cAAA;AACA,OAHA,MAGA;AACA,aAAA,cAAA,GAAA,CAAA,KAAA,cAAA;AACA;AACA;AAhEA;AApCA,CAAA","sourcesContent":["<template>\r\n    <v-row>\r\n        <v-expansion-panels>\r\n            <v-expansion-panel v-for=\"(item,i) in 1\" :key=\"i\">\r\n                <v-expansion-panel-header color=\"purple\" style=\"color:white;\">\r\n                    <v-row>\r\n                        <v-col cols=\"9\">\r\n                            <p>\r\n                                [Dominio]\r\n                                {{ dominio_data.nameknowledgedomain }}\r\n                            </p>\r\n                        </v-col>\r\n                        <v-col cols=\"3\">\r\n                            <!--Formulario para adição do módulo-->\r\n                            <v-dialog v-model=\"dialog_modulo\" persistent=\"persistent\" max-width=\"600px\">\r\n                                <template v-slot:activator=\"{ on }\">\r\n                                    <v-btn icon=\"icon\" color=\"white\" v-on=\"on\">\r\n                                        <v-icon>mdi-plus</v-icon>\r\n                                    </v-btn>\r\n                                </template>\r\n\r\n                                <ModuloDialog @close_or_save=\"close_or_save_modulo\" :domain=\"dominio\"/>\r\n\r\n                            </v-dialog>\r\n\r\n                            <!--Formulario para edição do domínio-->\r\n                            <v-dialog v-model=\"dialog_dominio\" persistent=\"persistent\" max-width=\"600px\">\r\n                                <template v-slot:activator=\"{ on }\">\r\n                                    <v-btn icon=\"icon\" color=\"white\" v-on=\"on\">\r\n                                        <v-icon>mdi-view-headline</v-icon>\r\n                                    </v-btn>\r\n                                </template>\r\n\r\n                                <DominioDialog\r\n                                    @close_or_save=\"close_or_save_dominio\"\r\n                                    @dominio_data=\"setDomainVariables\"\r\n                                    :domain=\"dominio_data\"/>\r\n\r\n                            </v-dialog>\r\n\r\n                        </v-col>\r\n                    </v-row>\r\n                </v-expansion-panel-header>\r\n                <v-expansion-panel-content>\r\n                    {{ dominio_data.subtitle }}\r\n                    <v-expansion-panels v-for=\"(modulo) in modulos\" :key=\"modulo.idmodule\">\r\n                        <v-expansion-panel>\r\n                            <v-expansion-panel-header>[Modulo]\r\n                                {{ modulo.namemodule }}</v-expansion-panel-header>\r\n\r\n                            <v-expansion-panel-content>{{ modulo.subtitle }}</v-expansion-panel-content>\r\n                        </v-expansion-panel>\r\n                    </v-expansion-panels>\r\n                </v-expansion-panel-content>\r\n            </v-expansion-panel>\r\n        </v-expansion-panels>\r\n    </v-row>\r\n</template>\r\n<script>\r\n    import DominioDialog from \"./concept_model/DominioDialog\";\r\n    import ModuloDialog from \"./concept_model/ModuloDialog\";\r\n    // import ConceitoDialog from \"./concept_model/ConceitoDialog\";\r\n    import axios from 'axios'\r\n    import Cookie from \"js-cookie\";\r\n    export default {\r\n        name: \"Panels\",\r\n        components: {\r\n            ModuloDialog,\r\n            DominioDialog\r\n        },\r\n        props: [\"dominio\"],\r\n        data: () => ({\r\n            valid_modulo: true,\r\n            dialog_modulo: false,\r\n            dialog_dominio: false,\r\n            select: null,\r\n            newItems: [\r\n                \"Módulo\", \"SubMódulo\"\r\n            ],\r\n            modulos: \"\",\r\n            checkbox: false,\r\n            /*ATRIBUTOS DO DOMINIO*/\r\n            dominio_data: {}\r\n        }),\r\n        mounted() {\r\n            /* Função que \"atrasa\" atualização da variável para conseguir pegar dados do\r\n     * props:[]\r\n     */\r\n\r\n            var vm = this;\r\n            setTimeout(function () {\r\n                vm.setDomainVariables(vm.dominio);\r\n            }, 1000);\r\n\r\n        },\r\n        computed: {\r\n            nomeDominioPanel: function () {\r\n                return this.domainName;\r\n            }\r\n        },\r\n        methods: {\r\n            validate() {\r\n                if (this.domainName && this.domainContentTitle && this.domainAuthorsName) {\r\n                    this.putDominio();\r\n                    this.dialog_dominio = false;\r\n                }\r\n            },\r\n            resetValidation() {\r\n                this\r\n                    .$refs\r\n                    .form\r\n                    .resetValidation();\r\n            },\r\n            reset() {\r\n                this.dialog_dominio = false;\r\n                this.setDomainVariables();\r\n            },\r\n            setDomainVariables(dominio_data) {\r\n                console.log(dominio_data);\r\n                this.dominio_data = dominio_data;\r\n                this.modulos = this.dominio_data.modules;\r\n            },\r\n            getDominio() {\r\n                var vm = this\r\n                var csrftoken = Cookie.get(\"csrftoken\");\r\n                var headers = {\r\n                    \"X-CSRFTOKEN\": csrftoken\r\n                };\r\n                axios\r\n                    .patch(\r\n                        \"http://127.0.0.1:8000/knowledgedomain/\" + this.dominio_data.idknowledgedomain +\r\n                                \"/\",\r\n                        {\r\n                            headers: headers\r\n                        },\r\n                        {\r\n                            auth: {\r\n                                username: \"admin\",\r\n                                password: \"admin\"\r\n                            }\r\n                        }\r\n                    )\r\n                    .then(function (resposta) {\r\n                        vm.setDomainVariables(resposta.data);\r\n                    });\r\n            },\r\n            close_or_save_modulo(value) {\r\n                var vm = this\r\n                if (value === \"save\") {\r\n                    setTimeout(function () {\r\n                        vm.getDominio();\r\n                    }, 1000);\r\n                    this.dialog_modulo = false;\r\n                } else {\r\n                    this.dialog_modulo = false;\r\n                }\r\n            },\r\n            close_or_save_dominio(value) {\r\n                if (value === \"save\") {\r\n\r\n                    this.dialog_dominio = !this.dialog_dominio;\r\n                } else {\r\n                    this.dialog_dominio = !this.dialog_dominio;\r\n                }\r\n            }\r\n        }\r\n    };\r\n</script>\r\n<style></style>"],"sourceRoot":"src/components"}]}