 height=\"49\" dark large @click=\"reset\">\r\n              Close\r\n              <v-icon dark right>mdi-close</v-icon>\r\n            </v-btn>\r\n            <v-btn color=\"success\" height=\"49\" dark large @click=\"validate\">\r\n              Save\r\n              <v-icon dark right>mdi-content-save</v-icon>\r\n            </v-btn>\r\n          </v-card-actions>\r\n        </v-card>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\n\r\n\r\nexport default {\r\n  name: \"ModuloDialog\",\r\n  props: ['dialog_modulo'],\r\n  data: () => ({\r\n    valid: true,\r\n    moduloTitle: \"\",\r\n    moduloTitleRules: [\r\n      v => !!v || \"É necessário descrever o título do submódulo\",\r\n      v =>\r\n        (v && v.length <= 40) ||\r\n        \"O título do módulo deve ter no máximo 40 caracteres\"\r\n    ],\r\n    moduloSubtitle: \"\",\r\n    moduloSubtitleRules: [\r\n      v => !!v || \"É necessário descrever o subtítulo do submódulo\",\r\n      v =>\r\n        (v && v.length <= 40) ||\r\n        \"O subtítulo do módulo deve ter no máximo 40 caracteres\"\r\n    ],\r\n    select: null,\r\n    newItems: [],\r\n    checkbox: false,\r\n    modulos: \"\",\r\n  }),\r\n  methods: {\r\n    postModulo() {\r\n      var vm = this;\r\n      axios\r\n        .post(\r\n          `http://localhost:8000/Module/`,\r\n          {\r\n            namemodule: this.moduloTitle,\r\n            subtitle: this.moduloSubtitle,\r\n            idknowledgedomain: this.idDomain\r\n          },\r\n          { auth: { username: \"admin\", password: \"admin\" } }\r\n        )\r\n        .then(function(resposta) {\r\n          vm.moduloTitle = resposta.data.namemodule;\r\n          vm.subTitle = resposta.data.subtitle;\r\n        });\r\n    },\r\n    validate() {\r\n      if(this.$refs.form.validate()){\r\n        this.$refs.form.validate();\r\n        this.postModulo();\r\n      }\r\n    },\r\n    reset() {\r\n      this.$emit('dialog_modulo', false);\r\n      this.$refs.form.reset();\r\n    },\r\n    resetValidation() {\r\n      this.$refs.form.resetValidation();\r\n    },\r\n  }\r\n};\r\n</script>"],"sourceRoot":"src/components/concept_model"}]}