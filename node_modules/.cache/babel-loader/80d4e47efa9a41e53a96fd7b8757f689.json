AAA,EAAA,CAAA,MAAA,CAAA,mBAAA;AACA,IAAA,EAAA,CAAA,kBAAA,GAAA,EAAA,CAAA,MAAA,CAAA,QAAA;AACA,GA3BA;AA4BA,EAAA,KAAA,EAAA;AACA,IAAA,MAAA,EAAA,kBAAA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,EAAA,CAAA,UAAA,GAAA,EAAA,CAAA,MAAA,CAAA,mBAAA;AACA,QAAA,EAAA,CAAA,kBAAA,GAAA,EAAA,CAAA,MAAA,CAAA,QAAA;AACA,OAHA;AAIA;AAPA,GA5BA;AAqCA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,wBACA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,MAAA,GAAA,GACA,GADA,CACA,sBAAA,KAAA,MAAA,CAAA,iBAAA,GAAA,GADA,EACA;AACA,QAAA,mBAAA,EAAA,KAAA,UADA;AAEA,QAAA,QAAA,EAAA,KAAA,kBAFA;AAGA,QAAA,WAAA,EAAA,KAAA,WAHA;AAIA,QAAA,SAAA,EAAA,KAAA,MAAA,CAAA;AAJA,OADA,EAOA,IAPA,CAOA,UAAA,QAAA,EAAA;AACA,QAAA,EAAA,CAAA,KAAA,CAAA,cAAA,EAAA,QAAA,CAAA,IAAA;AACA,OATA;AAUA,KAbA;AAcA,IAAA,QAdA,sBAcA;AACA,UAAA,KAAA,KAAA,CAAA,IAAA,CAAA,QAAA,EAAA,EAAA;AACA,aAAA,KAAA,CAAA,IAAA,CAAA,QAAA;AACA,aAAA,UAAA;AACA,aAAA,KAAA,CAAA,eAAA,EAAA,MAAA;AACA;AACA,KApBA;AAqBA,IAAA,KArBA,mBAqBA;AACA,WAAA,KAAA,CAAA,eAAA,EAAA,OAAA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,KAAA;AACA,KAxBA;AAyBA,IAAA,eAzBA,6BAyBA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,eAAA;AACA;AA3BA;AArCA,CAAA","sourcesContent":["<template>\n  <v-container class=\"pa-0\">\n    <v-card>\n      <v-card-title style=\"background-color:#63B0B0; color:white;\">\n        <span class=\"headline\">\n          <p>Edite o domínio selecionado.</p>\n        </span>\n      </v-card-title>\n      <v-card-text>\n        <v-form ref=\"form\" v-model=\"valid\" lazy-validation=\"lazy-validation\">\n          <v-text-field\n            v-model=\"domainName\"\n            :counter=\"100\"\n            :rules=\"domainNameRules\"\n            label=\"Dominío modelado\"\n            required=\"required\"\n          ></v-text-field>\n\n          <v-text-field\n            v-model=\"domainContentTitle\"\n            :rules=\"subtitleRules\"\n            label=\"Subtítulo para o conteúdo modelado\"\n            ref=\"refSubtitleDominio\"\n          ></v-text-field>\n        </v-form>\n      </v-card-text>\n      <v-card-actions>\n        <v-spacer></v-spacer>\n        <v-btn color=\"red\" height=\"49\" dark=\"dark\" large=\"large\" @click=\"reset\">\n          Cancelar\n          <v-icon dark=\"dark\" right=\"right\">mdi-close</v-icon>\n        </v-btn>\n        <v-btn\n          color=\"success\"\n          height=\"49\"\n          dark=\"dark\"\n          large=\"large\"\n          @click=\"validate\"\n          :disabled=\"!valid\"\n        >\n          Salvar\n          <v-icon dark=\"dark\" right=\"right\">mdi-content-save</v-icon>\n        </v-btn>\n      </v-card-actions>\n    </v-card>\n  </v-container>\n</template>\n\n<script>\nimport Api from \"@/services/Api\";\nexport default {\n  name: \"domainDialog\",\n  props: [\"domain\", \"dialog\"],\n  data: () => ({\n    domainName: \"\",\n    domainContentTitle: \"\",\n    subtitleRules: [\n      (v) =>\n        v.length <= 100 ||\n        \"Subtítulo do domínio deve ter no máximo 100 caracteres\",\n    ],\n    lastversion: \"\",\n    valid: true,\n    dialog_dominio: false,\n    /*REGRAS PARA VALIDAÇÃO DO FORMULÁRIO DOMÍNIO*/\n    domainNameRules: [\n      (v) => !!v || \"É necessário descrever o nome do domínio modelado\",\n      (v) =>\n        (v && v.length <= 100) ||\n        \"Nome do domínio deve ter no máximo 100 caracteres\",\n    ],\n    domains: \"\",\n  }),\n  mounted() {\n    var vm = this;\n    vm.domainName = vm.domain.nameknowledgedomain;\n    vm.domainContentTitle = vm.domain.subtitle;\n  },\n  watch: {\n    dialog: function () {\n      var vm = this;\n      this.$nextTick(function () {\n        vm.domainName = vm.domain.nameknowledgedomain;\n        vm.domainContentTitle = vm.domain.subtitle;\n      });\n    },\n  },\n  methods: {\n    putDominio() {\n      var vm = this;\n      Api()\n        .put(\"/knowledgedomain/\" + this.domain.idknowledgedomain + \"/\", {\n          nameknowledgedomain: this.domainName,\n          subtitle: this.domainContentTitle,\n          lastversion: this.lastversion,\n          fk_iduser: this.domain.fk_iduser,\n        })\n        .then(function (resposta) {\n          vm.$emit(\"dominio_data\", resposta.data);\n        });\n    },\n    validate() {\n      if (this.$refs.form.validate()) {\n        this.$refs.form.validate();\n        this.putDominio();\n        this.$emit(\"close_or_save\", \"save\");\n      }\n    },\n    reset() {\n      this.$emit(\"close_or_save\", \"close\");\n      this.$refs.form.reset();\n    },\n    resetValidation() {\n      this.$refs.form.resetValidation();\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/components/concept_model"}]}