IAAA,EAAA,IACA,mDAFA;AAAA,OAFA,CAjBA;AAuBA,MAAA,KAAA,EAAA,EAvBA;AAwBA,MAAA,UAAA,EAAA,CACA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,CAAA,IAAA,iCAAA;AAAA,OADA,EAEA,UAAA,CAAA;AAAA,eAAA,YAAA,IAAA,CAAA,CAAA,KAAA,uCAAA;AAAA,OAFA,CAxBA;AA4BA,MAAA,QAAA,EAAA,EA5BA;AA6BA,MAAA,aAAA,EAAA,CAAA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,CAAA,IAAA,gCAAA;AAAA,OAAA,CA7BA;AA8BA,MAAA,eAAA,EAAA,EA9BA;AA+BA,MAAA,YAAA,EAAA;AA/BA,KAAA;AAiCA,GApCA;AAqCA,EAAA,QAAA,EAAA;AACA,IAAA,wBADA,sCACA;AAAA;;AACA,aAAA;AAAA,eACA,KAAA,CAAA,QAAA,KAAA,KAAA,CAAA,eAAA,IAAA,4BADA;AAAA,OAAA;AAEA;AAJA,GArCA;AA2CA,EAAA,OAAA,EAAA;AACA,IAAA,QADA,sBACA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CACA,8BADA,EAEA;AACA,kBAAA,QAAA,EAAA,MAAA,CAAA,QAAA,CAAA,WAAA,EADA;AAEA,kBAAA,KAAA,EAAA,MAAA,CAAA,KAFA;AAGA,kBAAA,UAAA,EAAA,MAAA,CAAA,IAHA;AAIA,kBAAA,SAAA,EAAA,MAAA,CAAA,QAJA;AAKA,kBAAA,QAAA,EAAA,MAAA,CAAA,QALA;AAMA,kBAAA,SAAA,EAAA;AANA,iBAFA,EAUA;AACA,kBAAA,IAAA,EAAA;AACA,oBAAA,QAAA,EAAA,OADA;AAEA,oBAAA,QAAA,EAAA;AAFA;AADA,iBAVA,CADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAkBA,KAnBA;AAoBA,IAAA,QApBA,sBAoBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,QAAA,EADA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA,uBAGA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,QAAA,EAHA;;AAAA;AAAA;AAAA,uBAIA,MAAA,CAAA,QAAA,EAJA;;AAAA;AAKA,gBAAA,MAAA,CAAA,YAAA,GAAA,gCAAA;AACA,gBAAA,MAAA,CAAA,MAAA,GAAA,KAAA;AANA;AAAA;;AAAA;AAAA;AAAA;AAQA,gBAAA,MAAA,CAAA,YAAA,GAAA,sBAAA;;AARA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA,KA/BA;AAgCA,IAAA,UAhCA,wBAgCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,aAAA,EAAA;AACA,kBAAA,QAAA,EAAA,MAAA,CAAA,QADA;AAEA,kBAAA,QAAA,EAAA,MAAA,CAAA;AAFA,iBAAA,CADA;;AAAA;AAAA;AAAA,uBAKA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,kBAAA,IAAA,EAAA;AADA,iBAAA,CALA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA;AAxCA;AA3CA,CAAA","sourcesContent":["<template>\r\n  <v-card class=\"elevation-12\">\r\n    <v-toolbar color=\"black\" flat>\r\n      <v-toolbar-title style=\"color:#FFCC00;\">Crie sua conta no Educom.ML</v-toolbar-title>\r\n      <v-spacer></v-spacer>\r\n      <v-img class=\"mt-1\" src=\"../assets/logo.png\" style=\"width: 20px;\"></v-img>\r\n      <v-tooltip bottom>\r\n        <span>Source</span>\r\n      </v-tooltip>\r\n    </v-toolbar>\r\n    <v-card-text>\r\n      <v-form style=\"margin-bottom: -20px;\" ref=\"form\">\r\n        <v-text-field\r\n          v-model=\"username\"\r\n          label=\"Username\"\r\n          name=\"username\"\r\n          :rules=\"usernameRules\"\r\n          prepend-icon=\"mdi-account\"\r\n          required\r\n        ></v-text-field>\r\n\r\n        <v-text-field\r\n          v-model=\"name\"\r\n          label=\"Nome\"\r\n          name=\"name\"\r\n          :rules=\"nameRules\"\r\n          prepend-icon=\"mdi-account\"\r\n          required\r\n        ></v-text-field>\r\n\r\n        <v-text-field\r\n          v-model=\"lastname\"\r\n          label=\"Sobrenome\"\r\n          name=\"lastname\"\r\n          :rules=\"lastnameRules\"\r\n          prepend-icon=\"mdi-account\"\r\n          required\r\n        ></v-text-field>\r\n\r\n        <v-text-field\r\n          id=\"email\"\r\n          label=\"E-mail\"\r\n          name=\"email\"\r\n          :rules=\"emailRules\"\r\n          v-model=\"email\"\r\n          prepend-icon=\"mdi-email\"\r\n          type=\"email\"\r\n          required\r\n        ></v-text-field>\r\n\r\n        <v-text-field\r\n          id=\"password\"\r\n          label=\"Senha\"\r\n          name=\"password\"\r\n          v-model=\"password\"\r\n          :rules=\"passwordRules\"\r\n          type=\"password\"\r\n          prepend-icon=\"mdi-lock\"\r\n          required\r\n        ></v-text-field>\r\n\r\n        <v-text-field\r\n          id=\"passwordConfirm\"\r\n          label=\"Confirmação de Senha\"\r\n          name=\"passwordConfirm\"\r\n          v-model=\"passwordConfirm\"\r\n          type=\"password\"\r\n          :rules=\"[passwordConfirmationRule]\"\r\n          prepend-icon=\"mdi-lock\"\r\n          required\r\n        ></v-text-field>\r\n      </v-form>\r\n    </v-card-text>\r\n    <v-card-actions>\r\n      <v-spacer></v-spacer>\r\n      {{ messageError }}\r\n      <v-btn color=\"primary\" @click=\"validate()\" large class=\"mr-4\">Cadastrar</v-btn>\r\n    </v-card-actions>\r\n    <!--\r\n    <v-card-text class=\"mt-3\">\r\n      <p style=\"text-align: center; font-size: 1.2em;\">\r\n        Já possui conta?\r\n        <a href=\"#\" style=\"text-decoration: none;\">\r\n          <strong>ENTRE</strong>\r\n        </a>.\r\n      </p>\r\n    </v-card-text>\r\n    -->\r\n    <!--\r\n    <div class=\"text-center\">\r\n      <v-dialog v-model=\"dialog\" width=\"500\">\r\n        <v-card>\r\n          <v-card-title\r\n            class=\"headline primary\"\r\n            primary-title\r\n            style=\"color:white;\"\r\n          >Cadastro Realizado!</v-card-title>\r\n          <v-card-text class=\"mt-3\" style=\"font-size: 1.3em;\">Para prosseguir, faça seu login!</v-card-text>\r\n          <v-divider></v-divider>\r\n          <v-card-actions>\r\n            <v-spacer></v-spacer>\r\n            <v-btn color=\"primary\" text>Ok</v-btn>\r\n          </v-card-actions>\r\n        </v-card>\r\n      </v-dialog>\r\n    </div>\r\n    -->\r\n  </v-card>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"SignUp\",\r\n  data() {\r\n    return {\r\n      dialog: false,\r\n      username: \"\",\r\n      usernameRules: [\r\n        v => !!v || \"É necessário inserir seu username\",\r\n        v =>\r\n          (v && v.length <= 25) ||\r\n          \"Seu username deve possuir menos de 25 caracteres\"\r\n      ],\r\n      name: \"\",\r\n      nameRules: [\r\n        v => !!v || \"É necessário inserir seu nome\",\r\n        v =>\r\n          (v && v.length <= 30) ||\r\n          \"Seu nome deve possuir menos de 30 caracteres\"\r\n      ],\r\n      lastname: \"\",\r\n      lastnameRules: [\r\n        v => !!v || \"É necessário inserir seu sobrenome\",\r\n        v =>\r\n          (v && v.length <= 30) ||\r\n          \"Seu sobrenome deve possuir menos de 30 caracteres\"\r\n      ],\r\n      email: \"\",\r\n      emailRules: [\r\n        v => !!v || \"É necessário inserir seu e-mail\",\r\n        v => /.+@.+\\..+/.test(v) || \"É necessário inserir um e-mail válido\"\r\n      ],\r\n      password: \"\",\r\n      passwordRules: [v => !!v || \"É necessário inserir sua senha\"],\r\n      passwordConfirm: \"\",\r\n      messageError: \"\"\r\n    };\r\n  },\r\n  computed: {\r\n    passwordConfirmationRule() {\r\n      return () =>\r\n        this.password === this.passwordConfirm || \"As senhas devem ser iguais\";\r\n    }\r\n  },\r\n  methods: {\r\n    async postUser() {\r\n      await this.axios.post(\r\n        \"http://localhost:8000/users/\",\r\n        {\r\n          username: this.username.toLowerCase(),\r\n          email: this.email,\r\n          first_name: this.name,\r\n          last_name: this.lastname,\r\n          password: this.password,\r\n          is_active: false\r\n        },\r\n        {\r\n          auth: {\r\n            username: \"admin\",\r\n            password: \"admin\"\r\n          }\r\n        }\r\n      );\r\n    },\r\n    async validate() {\r\n      if (this.$refs.form.validate()) {\r\n        try {\r\n          await this.$refs.form.validate();\r\n          await this.postUser();\r\n          this.messageError = \"Porfavor verifique o seu email\";\r\n          this.dialog = false;\r\n        } catch (err) {\r\n          this.messageError = \"O username já existe\";\r\n        }\r\n      }\r\n    },\r\n    async firstLogin() {\r\n      await this.$store.dispatch(\"obtainToken\", {\r\n        username: this.username,\r\n        password: this.password\r\n      });\r\n      await this.$router.push({\r\n        name: \"home\"\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>"],"sourceRoot":"src/components"}]}