AJA;AAKA,QAAA,IAAA,EAAA,KAAA,YAAA,CAAA,IALA;AAMA,QAAA,QAAA,EAAA,KAAA,YAAA,CAAA,IANA;AAOA,QAAA,UAAA,EAAA,EAPA;AAQA,QAAA,WAAA,EAAA,KAAA,iBARA;AASA,QAAA,IAAA,EAAA,IATA;AAUA,QAAA,QAAA,EAAA,IAVA;AAWA,QAAA,SAAA,EAAA,IAXA;AAYA,QAAA,OAAA,EAAA;AAZA,OAAA;AAcA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,IAAA;;AACA,UAAA,KAAA,IAAA,KAAA,SAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,WAAA,EAAA;AACA,UAAA,oBAAA,EAAA,KAAA,UAAA,CAAA;AADA,SAAA;AAGA,OAJA,MAIA,IAAA,KAAA,IAAA,KAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,WAAA,EAAA;AACA,UAAA,SAAA,EAAA,KAAA,UAAA,CAAA;AADA,SAAA;AAGA,OAJA,MAIA,IAAA,KAAA,IAAA,KAAA,UAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,WAAA,EAAA;AACA,UAAA,UAAA,EAAA,KAAA,UAAA,CAAA;AADA,SAAA;AAGA;;AAEA,MAAA,OAAA,CAAA,GAAA,CAAA,WAAA;AAEA,MAAA,KAAA,CACA,IADA,uCACA,WADA,EACA;AACA,QAAA,IAAA,EAAA;AACA,UAAA,QAAA,EAAA,OADA;AAEA,UAAA,QAAA,EAAA;AAFA;AADA,OADA,EAOA,IAPA,CAOA;AAAA;AAAA;AACA;;AAEA,OAVA;AAWA,KAhDA;AAiDA,IAAA,QAjDA,sBAiDA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,YAAA;AAEA,WAAA,eAAA;AAEA,MAAA,QAAA,CACA,OADA,GAEA,GAFA,CAEA,KAAA,YAAA,CAAA,IAFA,EAGA,GAHA,CAGA,KAAA,YAHA;AAIA,WAAA,KAAA,CAAA,OAAA;AACA;AA3DA;AAdA,CAAA","sourcesContent":["<template>\r\n  <v-container class=\"pa-0\">\r\n    <v-card>\r\n      <v-card-title style=\"background-color:#63B0B0; color:white;\">\r\n        <span class=\"headline\">\r\n          <p>Importe sua imagem.</p>\r\n        </span>\r\n      </v-card-title>\r\n      <v-card-text>\r\n        <!--Formulario para adição de submódulo ou conceito-->\r\n\r\n        <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\r\n          <v-text-field\r\n            v-model=\"imagemDescription\"\r\n            :rules=\"imagemDescriptionRules\"\r\n            label=\"Utilize uma descrição para a imagem\"\r\n            required\r\n          ></v-text-field>\r\n        </v-form>\r\n        <v-spacer></v-spacer>\r\n        <v-file-input\r\n          label=\"UPLOAD IMAGE\"\r\n          filled\r\n          v-model=\"imagemObject\"\r\n          accept=\"image/*\"\r\n          prepend-icon=\"mdi-camera\"\r\n        ></v-file-input>\r\n        <v-spacer></v-spacer>\r\n      </v-card-text>\r\n      <v-card-actions>\r\n        <v-spacer></v-spacer>\r\n        <v-btn color=\"red\" height=\"49\" dark large @click=\"reset\">\r\n          Close\r\n          <v-icon dark right>mdi-close</v-icon>\r\n        </v-btn>\r\n        <v-btn color=\"success\" height=\"49\" dark large @click=\"validate\">\r\n          Save\r\n          <v-icon dark right>mdi-content-save</v-icon>\r\n        </v-btn>\r\n      </v-card-actions>\r\n    </v-card>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport * as firebase from \"firebase\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"ImageDialog\",\r\n  props: [\"optionCall\", \"type\"],\r\n  data: () => ({\r\n    valid: true,\r\n    imagemDescription: \"\",\r\n    imagemObject: {},\r\n    imagemDescriptionRules: [\r\n      v => !!v || \"É necessário descrever a imagem a ser inserida\",\r\n      v =>\r\n        (v && v.length <= 50) ||\r\n        \"A descrição da imagem deve ter no máximo 50 caracteres\"\r\n    ]\r\n  }),\r\n  methods: {\r\n    reset() {\r\n      this.$emit(\"close\");\r\n    },\r\n    postMobileMedia() {\r\n      // var vm = this;\r\n      var mobilemedia = {\r\n        label: this.imagemDescription,\r\n        fk_idmediatype: \"http://localhost:8000/mediatype/1/\",\r\n        difficultyLevel: null,\r\n        learningStyle: null,\r\n        path: this.imagemObject.name,\r\n        namefile: this.imagemObject.name,\r\n        resolution: \"\",\r\n        description: this.imagemDescription,\r\n        time: null,\r\n        textfull: null,\r\n        textshort: null,\r\n        urllink: null\r\n      };\r\n      console.log(this.type);\r\n      if (this.type === \"dominio\") {\r\n        Object.assign(mobilemedia, {\r\n          fk_idknowledgedomain: this.optionCall.url\r\n        });\r\n      } else if (this.type === \"modulo\") {\r\n        Object.assign(mobilemedia, {\r\n          fk_module: this.optionCall.url\r\n        });\r\n      } else if (this.type === \"conceito\") {\r\n        Object.assign(mobilemedia, {\r\n          fk_concept: this.optionCall.url\r\n        });\r\n      }\r\n\r\n      console.log(mobilemedia);\r\n\r\n      axios\r\n        .post(`http://localhost:8000/mobilemedia/`, mobilemedia, {\r\n          auth: {\r\n            username: \"admin\",\r\n            password: \"admin\"\r\n          }\r\n        })\r\n        .then(function(/*resposta*/) {\r\n          /*vm.moduloTitle = resposta.data.namemodule;\r\n                    vm.subTitle = resposta.data.subtitle;*/\r\n        });\r\n    },\r\n    validate() {\r\n      console.log(this.imagemObject);\r\n\r\n      this.postMobileMedia();\r\n\r\n      firebase\r\n        .storage()\r\n        .ref(this.imagemObject.name)\r\n        .put(this.imagemObject);\r\n      this.$emit(\"close\");\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components/instructional_model"}]}