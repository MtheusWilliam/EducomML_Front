AA,kBAAA,CAAA,EAAA,CAAA,OAAA;AACA,KAFA,EAEA,IAFA,CAAA;AAGA,GA5BA;AA6BA,EAAA,QAAA,EAAA;AACA,IAAA,gBAAA,EAAA,4BAAA;AACA,aAAA,KAAA,UAAA;AACA;AAHA,GA7BA;AAkCA,EAAA,OAAA,EAAA;AACA,IAAA,QADA,sBACA;AACA,UACA,KAAA,UAAA,IACA,KAAA,kBADA,IAEA,KAAA,iBAHA,EAIA;AACA,aAAA,UAAA;AACA,aAAA,cAAA,GAAA,KAAA;AACA;AACA,KAVA;AAWA,IAAA,eAXA,6BAWA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,eAAA;AACA,KAbA;AAcA,IAAA,KAdA,mBAcA;AACA,WAAA,cAAA,GAAA,KAAA;AACA,WAAA,kBAAA;AACA,KAjBA;AAkBA,IAAA,kBAlBA,8BAkBA,YAlBA,EAkBA;AACA,WAAA,YAAA,GAAA,YAAA;AACA,WAAA,OAAA,GAAA,KAAA,YAAA,CAAA,OAAA;AACA,KArBA;AAsBA,IAAA,UAtBA,wBAsBA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,UAAA,SAAA,GAAA,MAAA,CAAA,GAAA,CAAA,WAAA,CAAA;AACA,UAAA,OAAA,GAAA;AACA,uBAAA;AADA,OAAA;AAGA,MAAA,KAAA,CACA,KADA,CAEA,2CACA,KAAA,YAAA,CAAA,iBADA,GAEA,GAJA,EAKA;AACA,QAAA,OAAA,EAAA;AADA,OALA,EAQA;AACA,QAAA,IAAA,EAAA;AACA,UAAA,QAAA,EAAA,OADA;AAEA,UAAA,QAAA,EAAA;AAFA;AADA,OARA,EAeA,IAfA,CAeA,UAAA,QAAA,EAAA;AACA,QAAA,EAAA,CAAA,kBAAA,CAAA,QAAA,CAAA,IAAA;AACA,OAjBA;AAkBA,KA9CA;AA+CA,IAAA,oBA/CA,gCA+CA,KA/CA,EA+CA;AACA,UAAA,EAAA,GAAA,IAAA;;AACA,UAAA,KAAA,KAAA,MAAA,EAAA;AACA,QAAA,UAAA,CAAA,YAAA;AACA,UAAA,EAAA,CAAA,UAAA;AACA,SAFA,EAEA,IAFA,CAAA;AAGA,aAAA,aAAA,GAAA,KAAA;AACA,OALA,MAKA,IAAA,KAAA,KAAA,OAAA,EAAA;AACA,aAAA,MAAA,GAAA,EAAA;AACA,aAAA,aAAA,GAAA,KAAA;AACA;AACA,KA1DA;AA2DA,IAAA,qBA3DA,mCA2DA;AACA,WAAA,cAAA,GAAA,CAAA,KAAA,cAAA;AACA,KA7DA;AA8DA,IAAA,SA9DA,qBA8DA,KA9DA,EA8DA;AACA,WAAA,MAAA,GAAA,KAAA;AACA,WAAA,aAAA,GAAA,CAAA,KAAA,aAAA;AACA;AAjEA;AAlCA,CAAA","sourcesContent":["<template>\r\n  <v-row>\r\n    <v-expansion-panels>\r\n      <v-expansion-panel v-for=\"(item,i) in 1\" :key=\"i\">\r\n        <v-expansion-panel-header color=\"purple\" style=\"color:white;\">\r\n          <v-row>\r\n            <v-col cols=\"9\">\r\n              <p>\r\n                [Dominio]\r\n                {{ dominio_data.nameknowledgedomain }}\r\n              </p>\r\n            </v-col>\r\n            <v-col cols=\"3\">\r\n              <!--Formulario para adição do módulo-->\r\n              <v-dialog v-model=\"dialog_modulo\" persistent=\"persistent\" max-width=\"600px\">\r\n                <template v-slot:activator=\"{ on }\">\r\n                  <v-btn icon=\"icon\" color=\"white\" v-on=\"on\">\r\n                    <v-icon>mdi-plus</v-icon>\r\n                  </v-btn>\r\n                </template>\r\n\r\n                <ModuloDialog\r\n                  :module=\"modulo\"\r\n                  @close_or_save=\"close_or_save_modulo\"\r\n                  :domain=\"dominio\"\r\n                />\r\n              </v-dialog>\r\n\r\n              <!--Formulario para edição do domínio-->\r\n              <v-dialog v-model=\"dialog_dominio\" persistent=\"persistent\" max-width=\"600px\">\r\n                <template v-slot:activator=\"{ on }\">\r\n                  <v-btn icon=\"icon\" color=\"white\" v-on=\"on\">\r\n                    <v-icon>mdi-view-headline</v-icon>\r\n                  </v-btn>\r\n                </template>\r\n\r\n                <DominioDialog\r\n                  @close_or_save=\"close_or_save_dominio\"\r\n                  @dominio_data=\"setDomainVariables\"\r\n                  :domain=\"dominio_data\"\r\n                />\r\n              </v-dialog>\r\n            </v-col>\r\n          </v-row>\r\n        </v-expansion-panel-header>\r\n        <v-expansion-panel-content>\r\n          {{ dominio_data.subtitle }}\r\n          <v-expansion-panels v-for=\"(modulo) in modulos\" :key=\"modulo.idmodule\">\r\n            <v-expansion-panel>\r\n              <v-expansion-panel-header color=\"primary\" style=\"color:white;\">\r\n                <v-row>\r\n                  <v-col cols=\"9\">\r\n                    <p>\r\n                      [Modulo]\r\n                      {{ modulo.namemodule }}\r\n                    </p>\r\n                  </v-col>\r\n                  <v-col cols=\"3\">\r\n                    <!--Formulario para adição de submódulo ou conceito-->\r\n\r\n                    <!--Formulario para edição do modulo-->\r\n                    <v-btn icon=\"icon\" color=\"white\" @click=\"setmodulo(modulo)\">\r\n                      <v-icon>mdi-view-headline</v-icon>\r\n                    </v-btn>\r\n                  </v-col>\r\n                </v-row>\r\n              </v-expansion-panel-header>\r\n\r\n              <v-expansion-panel-content>{{ modulo.subtitle }}</v-expansion-panel-content>\r\n            </v-expansion-panel>\r\n          </v-expansion-panels>\r\n        </v-expansion-panel-content>\r\n      </v-expansion-panel>\r\n    </v-expansion-panels>\r\n  </v-row>\r\n</template>\r\n<script>\r\nimport DominioDialog from \"./concept_model/DominioDialog\";\r\nimport ModuloDialog from \"./concept_model/ModuloDialog\";\r\n// import ConceitoDialog from \"./concept_model/ConceitoDialog\";\r\nimport axios from \"axios\";\r\nimport Cookie from \"js-cookie\";\r\nexport default {\r\n  name: \"Panels\",\r\n  components: {\r\n    ModuloDialog,\r\n    DominioDialog\r\n  },\r\n  props: [\"dominio\"],\r\n  data: () => ({\r\n    modulo: \"\",\r\n    valid_modulo: true,\r\n    dialog_modulo: false,\r\n    dialog_modulo_edit: false,\r\n    dialog_dominio: false,\r\n    select: null,\r\n    newItems: [\"Módulo\", \"SubMódulo\"],\r\n    modulos: \"\",\r\n    checkbox: false,\r\n    /*ATRIBUTOS DO DOMINIO*/\r\n    dominio_data: {}\r\n  }),\r\n  mounted() {\r\n    /* Função que \"atrasa\" atualização da variável para conseguir pegar dados do\r\n     * props:[]\r\n     */\r\n    var vm = this;\r\n    setTimeout(function() {\r\n      vm.setDomainVariables(vm.dominio);\r\n    }, 1000);\r\n  },\r\n  computed: {\r\n    nomeDominioPanel: function() {\r\n      return this.domainName;\r\n    }\r\n  },\r\n  methods: {\r\n    validate() {\r\n      if (\r\n        this.domainName &&\r\n        this.domainContentTitle &&\r\n        this.domainAuthorsName\r\n      ) {\r\n        this.putDominio();\r\n        this.dialog_dominio = false;\r\n      }\r\n    },\r\n    resetValidation() {\r\n      this.$refs.form.resetValidation();\r\n    },\r\n    reset() {\r\n      this.dialog_dominio = false;\r\n      this.setDomainVariables();\r\n    },\r\n    setDomainVariables(dominio_data) {\r\n      this.dominio_data = dominio_data;\r\n      this.modulos = this.dominio_data.modules;\r\n    },\r\n    getDominio() {\r\n      var vm = this;\r\n      var csrftoken = Cookie.get(\"csrftoken\");\r\n      var headers = {\r\n        \"X-CSRFTOKEN\": csrftoken\r\n      };\r\n      axios\r\n        .patch(\r\n          \"http://127.0.0.1:8000/knowledgedomain/\" +\r\n            this.dominio_data.idknowledgedomain +\r\n            \"/\",\r\n          {\r\n            headers: headers\r\n          },\r\n          {\r\n            auth: {\r\n              username: \"admin\",\r\n              password: \"admin\"\r\n            }\r\n          }\r\n        )\r\n        .then(function(resposta) {\r\n          vm.setDomainVariables(resposta.data);\r\n        });\r\n    },\r\n    close_or_save_modulo(value) {\r\n      var vm = this;\r\n      if (value === \"save\") {\r\n        setTimeout(function() {\r\n          vm.getDominio();\r\n        }, 1000);\r\n        this.dialog_modulo = false;\r\n      } else if (value === \"close\") {\r\n        this.modulo = \"\";\r\n        this.dialog_modulo = false;\r\n      }\r\n    },\r\n    close_or_save_dominio() {\r\n      this.dialog_dominio = !this.dialog_dominio;\r\n    },\r\n    setmodulo(value) {\r\n      this.modulo = value;\r\n      this.dialog_modulo = !this.dialog_modulo;\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style></style>"],"sourceRoot":"src/components"}]}