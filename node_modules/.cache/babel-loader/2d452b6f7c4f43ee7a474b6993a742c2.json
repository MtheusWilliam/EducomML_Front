DA;AAhBA,KAAA;AAAA,GAHA;AAyBA,EAAA,OAAA,EAAA;AACA,IAAA,wBADA,sCACA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,EADA,GACA,KADA;AAAA;AAAA,uBAEA,KAAA,CACA,IADA,2CAGA;AACA,kBAAA,mBAAA,EAAA,KAAA,CAAA,aADA;AAEA,kBAAA,0BAAA,EAAA,KAAA,CAAA,oBAFA;AAGA,kBAAA,sBAAA,gDAHA;AAIA,kBAAA,YAAA,EAAA,KAAA,CAAA,OAAA,CAAA;AAJA,iBAHA,EASA;AACA,kBAAA,IAAA,EAAA;AACA,oBAAA,QAAA,EAAA,OADA;AAEA,oBAAA,QAAA,EAAA;AAFA;AADA,iBATA,EAgBA,IAhBA,CAgBA,UAAA,QAAA,EAAA;AACA,kBAAA,EAAA,CAAA,WAAA,GAAA,QAAA,CAAA,IAAA,CAAA,GAAA;AACA,iBAlBA,CAFA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAqBA,KAtBA;AAuBA,IAAA,iBAvBA,+BAuBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,EADA,GACA,MADA;;AAEA,gBAAA,MAAA,CAAA,aAAA,CAAA,OAAA;AAAA,qFAAA,kBAAA,OAAA,EAAA,KAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mCACA,KAAA,CAAA,IAAA,0CAEA;AACA,8BAAA,KAAA,EAAA,KAAA,GAAA,CADA;AAEA,8BAAA,WAAA,EAAA,OAAA,CAAA,WAFA;AAGA,8BAAA,kBAAA,EAAA,EAAA,CAAA;AAHA,6BAFA,EAOA;AACA,8BAAA,IAAA,EAAA;AACA,gCAAA,QAAA,EAAA,OADA;AAEA,gCAAA,QAAA,EAAA;AAFA;AADA,6BAPA,CADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mBAAA;;AAAA;AAAA;AAAA;AAAA;;AAFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAkBA,KAzCA;AA2CA,IAAA,QA3CA,sBA2CA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,QAAA,EADA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAEA,MAAA,CAAA,wBAAA,EAFA;;AAAA;AAAA;AAAA,uBAGA,MAAA,CAAA,iBAAA,EAHA;;AAAA;AAIA,gBAAA,MAAA,CAAA,aAAA,GAAA,EAAA;;AACA,gBAAA,MAAA,CAAA,KAAA,CAAA,OAAA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KAlDA;AAmDA,IAAA,KAnDA,mBAmDA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,KAAA;AACA,WAAA,aAAA,GAAA,EAAA;AACA,WAAA,KAAA,CAAA,OAAA;AACA,KAvDA;AAwDA,IAAA,QAxDA,sBAwDA;AACA,WAAA,aAAA,CAAA,IAAA,CAAA;AACA,QAAA,EAAA,EAAA,KAAA,aAAA,CAAA,MAAA,GAAA,CADA;AAEA,QAAA,WAAA,EAAA;AAFA,OAAA;AAIA,KA7DA;AA8DA,IAAA,WA9DA,uBA8DA,OA9DA,EA8DA;AACA,UAAA,OAAA,IAAA,CAAA,EAAA;AACA,aAAA,aAAA,CAAA,KAAA;AACA,OAFA,MAEA;AACA,aAAA,aAAA,CAAA,MAAA,CAAA,OAAA,EAAA,CAAA;AACA;AACA;AApEA;AAzBA,CAAA","sourcesContent":["<template>\r\n  <v-container class=\"pa-0\">\r\n    <v-card>\r\n      <v-card-title style=\"background-color:#63B0B0; color:white;\">\r\n        <span class=\"headline\">\r\n          <p>Descreva o procedimento a ser criado.</p>\r\n        </span>\r\n      </v-card-title>\r\n      <v-card-text>\r\n        <v-form ref=\"form\" v-model=\"valid\" lazy-validation class=\"mt-3\">\r\n          <label class=\"pt-2\" style=\"font-size:1.3em;\" for=\"procedureNameArea\">\r\n            <strong>Escreva o nome do procedimento:</strong>\r\n          </label>\r\n          <v-text-field\r\n            id=\"procedureNameArea\"\r\n            v-model=\"procedureName\"\r\n            :counter=\"25\"\r\n            :rules=\"procedureNameRules\"\r\n            required\r\n            class=\"mt-2 mb-4\"\r\n          ></v-text-field>\r\n          <label class=\"pt-2\" style=\"font-size:1.3em;\" for=\"procedureDescriptionArea\">\r\n            <strong>Escreva a descrição do procedimento:</strong>\r\n          </label>\r\n          <v-textarea\r\n            id=\"procedureDescriptionArea\"\r\n            v-model=\"procedureDescription\"\r\n            :rules=\"procedureDescriptionRules\"\r\n            background-color=\"#F2F3F3\"\r\n            clearable\r\n            clear-icon=\"mdi-close-circle\"\r\n            class=\"mt-2\"\r\n            name=\"input-3-1\"\r\n            filled\r\n            auto-grow\r\n            required\r\n          ></v-textarea>\r\n          <v-app-bar color=\"#D2A64D\" dense dark height=\"45px\">\r\n            <v-toolbar-title style=\"font-size:1.2em;\">\r\n              <strong>Lista de fases do procedimento</strong>\r\n            </v-toolbar-title>\r\n          </v-app-bar>\r\n          <v-container>\r\n            <v-row style=\"height:50px;\">\r\n              <v-col>\r\n                <strong>\r\n                  <p style=\"font-size:1.2em;\">Descrição da fase</p>\r\n                </strong>\r\n              </v-col>\r\n              <v-col cols=\"1\">\r\n                <v-btn icon=\"icon\" class=\"mb-1\">\r\n                  <v-icon large class=\"mb-3\" color=\"primary\" @click=\"addPhase()\">mdi-plus-box</v-icon>\r\n                </v-btn>\r\n              </v-col>\r\n            </v-row>\r\n\r\n            <v-row v-for=\"(item,i) in phasesControl\" :key=\"i\">\r\n              <v-text-field\r\n                class=\"ml-3\"\r\n                v-model=\"phasesControl[i].description\"\r\n                :rules=\"phasesControlRules\"\r\n                label=\"Fase do procedimento\"\r\n                required\r\n              ></v-text-field>\r\n              <v-col cols=\"1\" class=\"mt-2\">\r\n                <v-btn icon=\"icon\" class=\"mb-1\">\r\n                  <v-icon large class=\"mb-1\" color=\"red\" @click=\"deletaPhase(i)\">mdi-minus-box</v-icon>\r\n                </v-btn>\r\n              </v-col>\r\n            </v-row>\r\n          </v-container>\r\n        </v-form>\r\n      </v-card-text>\r\n      <v-card-actions>\r\n        <v-spacer></v-spacer>\r\n        <v-btn color=\"red\" height=\"49\" dark large @click=\"reset()\">\r\n          Close\r\n          <v-icon dark right>mdi-close</v-icon>\r\n        </v-btn>\r\n        <v-btn color=\"success\" height=\"49\" dark large @click=\"validate()\">\r\n          Save\r\n          <v-icon dark right>mdi-content-save</v-icon>\r\n        </v-btn>\r\n      </v-card-actions>\r\n    </v-card>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"ProcedureDialog\",\r\n  props: [\"concept\"],\r\n  data: () => ({\r\n    valid: true,\r\n    auxItemInfo: \"\",\r\n    phasesControl: [],\r\n    phasesControlRules: [\r\n      v =>\r\n        !!v || \"É necessário inserir algum conteúdo nessa etapa do procedimento\"\r\n    ],\r\n    procedureName: \"\",\r\n    procedureNameRules: [\r\n      v => !!v || \"É necessário descrever o nome do procedimento\",\r\n      v =>\r\n        (v && v.length <= 25) ||\r\n        \"Nome do procedimento deve ter no máximo 25 caracteres\"\r\n    ],\r\n    procedureDescription: \"\",\r\n    procedureDescriptionRules: [\r\n      v =>\r\n        !!v ||\r\n        \"É necessário inserir uma descrição do procedimento a ser inserido\"\r\n    ]\r\n  }),\r\n  methods: {\r\n    async altera_Cria_Procedimento() {\r\n      var vm = this;\r\n      await axios\r\n        .post(\r\n          `http://localhost:8000/informationitem/`,\r\n          {\r\n            nameinformationitem: this.procedureName,\r\n            descriptioninformationitem: this.procedureDescription,\r\n            fk_informationitemtype: `http://localhost:8000/informationitemtype/4/`,\r\n            fk_idconcept: this.concept.url\r\n          },\r\n          {\r\n            auth: {\r\n              username: \"admin\",\r\n              password: \"admin\"\r\n            }\r\n          }\r\n        )\r\n        .then(function(resposta) {\r\n          vm.auxItemInfo = resposta.data.url;\r\n        });\r\n    },\r\n    async altera_Cria_Fases() {\r\n      var vm = this;\r\n      this.phasesControl.forEach(async function(element, index) {\r\n        await axios.post(\r\n          `http://localhost:8000/phaseprocedure/`,\r\n          {\r\n            order: index + 1,\r\n            description: element.description,\r\n            fk_informationitem: vm.auxItemInfo\r\n          },\r\n          {\r\n            auth: {\r\n              username: \"admin\",\r\n              password: \"admin\"\r\n            }\r\n          }\r\n        );\r\n      });\r\n    },\r\n\r\n    async validate() {\r\n      if (this.$refs.form.validate()) {\r\n        await this.altera_Cria_Procedimento();\r\n        await this.altera_Cria_Fases();\r\n        this.phasesControl = [];\r\n        this.$emit(\"close\");\r\n      }\r\n    },\r\n    reset() {\r\n      this.$refs.form.reset();\r\n      this.phasesControl = [];\r\n      this.$emit(\"close\");\r\n    },\r\n    addPhase() {\r\n      this.phasesControl.push({\r\n        id: this.phasesControl.length + 1,\r\n        description: null\r\n      });\r\n    },\r\n    deletaPhase(idPhase) {\r\n      if (idPhase == 0) {\r\n        this.phasesControl.shift();\r\n      } else {\r\n        this.phasesControl.splice(idPhase, 1);\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components/instructional_model"}]}